    "rules": {
      "gt0029": {
        "id": "gt0029",
        "priority": 7,
        "when": [
          "$gt0036|Age|!=null",
          "$gt0006|Current or former smoker|!=null",
          "$gt0007|Extrathoracic cancer diagnosis ≥5 years prior|!=null",
          "$gt0008|Upper lobe location of tumor|!=null",
          "$gt0010|Nodule spiculation|!=null",
          "$gt0028|Nodule diameter|!=null",
          "$gt0011|FDG-PET|!=null"
        ],
        "then": [
          "$gt0031|x|.magnitude=(0.0391*$gt0036.magnitude)+(0.1274*$gt0028.magnitude)+(0.7917*$gt0006.value)+(1.3388*$gt0007.value)+(0.7838*$gt0008.value)+(1.0407*$gt0010.value)-6.8272",
          "$gt0031|x|.precision=5"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 6,
        "when": [
          "$gt0031|x|!=null"
        ],
        "then": [
          "$gt0033|Probability of malignancy|.magnitude=(100*e^$gt0031.magnitude)/(1+e^$gt0031.magnitude)",
          "$gt0033|Probability of malignancy|.precision=1",
          "$gt0033|Probability of malignancy|.unit='%'"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 5,
        "when": [
          "$gt0011|FDG-PET|==local::at0027|Intense uptake|",
          "$gt0031|x|!=null"
        ],
        "then": [
          "$gt0032|y|.magnitude=(3.691*($gt0033.magnitude/100))+4.771-4.739",
          "$gt0032|y|.precision=5"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 4,
        "when": [
          "$gt0011|FDG-PET|==local::at0026|Moderate uptake|",
          "$gt0031|x|!=null"
        ],
        "then": [
          "$gt0032|y|.magnitude=(3.691*($gt0033.magnitude/100))+4.617-4.739",
          "$gt0032|y|.precision=5"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 3,
        "when": [
          "$gt0011|FDG-PET|==local::at0025|Faint uptake|",
          "$gt0031|x|!=null"
        ],
        "then": [
          "$gt0032|y|.magnitude=(3.691*($gt0033.magnitude/100))+2.322-4.739",
          "$gt0032|y|.precision=5"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 2,
        "when": [
          "$gt0011|FDG-PET|==local::at0024|No uptake|",
          "$gt0031|x|!=null"
        ],
        "then": [
          "$gt0032|y|.magnitude=(3.691*($gt0033.magnitude/100))-4.739",
          "$gt0032|y|.precision=5"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 1,
        "when": [
          "$gt0032|y|!=null"
        ],
        "then": [
          "$gt0034|Probability of malignancy with FDG-PET|.magnitude=100/(1+e^(0-$gt0032.magnitude))",
          "$gt0034|Probability of malignancy with FDG-PET|.precision=1",
          "$gt0034|Probability of malignancy with FDG-PET|.unit='%'"
        ]
      }
    }
  },
