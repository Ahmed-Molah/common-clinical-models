    "rules": {
      "gt0024": {
        "id": "gt0024",
        "priority": 8,
        "when": [
          "$gt0009|Serum Albumin|<1,gm/dl"
        ],
        "then": [
          "$gt0022|Warning Albumin|='Very low double-check.'"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 7,
        "when": [
          "$gt0009|Serum Albumin|>6,gm/dl",
          "$gt0009|Serum Albumin|<=7,gm/dl"
        ],
        "then": [
          "$gt0022|Warning Albumin|='Very high double-check.'"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 6,
        "when": [
          "$gt0009|Serum Albumin|>7,gm/dl"
        ],
        "then": [
          "$gt0022|Warning Albumin|='Too high please change to proceed.'"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 5,
        "when": [
          "$gt0011|Hemoglobin|==0,gm/dl"
        ],
        "then": [
          "$gt0023|Warning Hemoglobin|='Too low please change to proceed.'"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 4,
        "when": [
          "$gt0011|Hemoglobin|<4,gm/dl",
          "$gt0011|Hemoglobin|>0,gm/dl"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 3,
        "when": [
          "$gt0011|Hemoglobin|>20,gm/dl",
          "$gt0011|Hemoglobin|<=25,gm/dl"
        ],
        "then": [
          "$gt0023|Warning Hemoglobin|='Very high double-check.'"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 2,
        "when": [
          "$gt0011|Hemoglobin|>25,gm/dl"
        ],
        "then": [
          "$gt0023|Warning Hemoglobin|='Too high please change to proceed.'"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 1,
        "when": [
          "$gt0003|Treatment with enzalutamide planned|!=null",
          "$gt0005|PSA value|!=null",
          "$gt0007|Time since diagnosis|!=null",
          "$gt0009|Serum Albumin|!=null",
          "$gt0011|Hemoglobin|!=null",
          "$gt0009|Serum Albumin|.unit=='gm/dl'",
          "$gt0011|Hemoglobin|.unit=='gm/dl'",
          "$gt0013|Neutrophil to lymphocyte ratio <2.5|!=null",
          "$gt0014|LDH below upper limit of normal|!=null",
          "$gt0015|Pain 0-1|!=null",
          "$gt0016|<10 bone metastases on bone scan|!=null",
          "$gt0017|Alkaline phosphatase less than upper limit of normal|!=null",
          "$gt0018|Pattern of spread|!=null",
          "!fired($gt0026)",
          "!fired($gt0027)",
          "!fired($gt0030)"
        ],
        "then": [
          "$gt0020|Total score|.magnitude=($gt0003.value*18.1376)+((log($gt0005.magnitude)+2.3026)*9.52244917)+((275.9-$gt0007.magnitude)*0.1543483)+((4.9-$gt0009.magnitude)*16.0447656796)+((16.8-$gt0011.magnitude)*7.7519235)+($gt0013.value*17.613)+($gt0014.value*19.00635)+($gt0015.value*12.2248)+($gt0016.value*17.7533)+($gt0017.value*10.9685)+($gt0018.value*32.991)",
          "$gt0020|Total score|.unit='1'"
        ]
      }
    }
