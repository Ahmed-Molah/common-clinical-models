          }
        }
      }
    },
    "rules": {
      "gt0012": {
        "id": "gt0012",
        "priority": 3,
        "when": [
          "$gt0003|Fetal weight, gm|.unit=='oz(avdp)'"
        ],
        "then": [
          "$gt0009|Fetoplacental volume|.magnitude=$gt0003.magnitude*28.35*0.14",
          "$gt0009|Fetoplacental volume|.unit='ml'"
        ]
      },
      "gt0013": {
        "id": "gt0013",
        "priority": 2,
        "when": [
          "$gt0003|Fetal weight, gm|.unit=='gm'"
        ],
        "then": [
          "$gt0009|Fetoplacental volume|.magnitude=$gt0003.magnitude*0.14",
          "$gt0009|Fetoplacental volume|.unit='ml'"
        ]
      },
      "gt0014": {
        "id": "gt0014",
        "priority": 1,
        "when": [
          "$gt0004|Initial fetal hematocrit|!=null",
          "$gt0005|Goal fetal hematocrit|!=null",
          "$gt0006|Hematocrit of transfused RBCs|!=null"
        ],
        "then": [
          "$gt0008|Transfusion volume|.magnitude=$gt0009.magnitude*($gt0005.magnitude-$gt0004.magnitude)/$gt0006.magnitude",
          "$gt0008|Transfusion volume|.precision=1",
          "$gt0008|Transfusion volume|.unit='ml'"
        ]
      }
    }
  },
