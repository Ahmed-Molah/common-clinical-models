          }
        }
      }
    },
    "rules": {
      "gt0010": {
        "id": "gt0010",
        "priority": 5,
        "when": [
          "$gt0003|End stage renal disease|==local::at0005|Yes|"
        ],
        "then": [
          "$gt0009| Level of renal dysfunction|=local::at0034|ESRD, RIFLE-E|"
        ]
      },
      "gt0011": {
        "id": "gt0011",
        "priority": 4,
        "when": [
          "$gt0003|End stage renal disease|==local::at0006|No|",
          "$gt0004|Persistent acute kidney injury|==local::at0014|Yes|"
        ],
        "then": [
          "$gt0009| Level of renal dysfunction|=local::at0033|Loss of kidney function, RIFLE-L|"
        ]
      },
      "gt0012": {
        "id": "gt0012",
        "priority": 3,
        "when": [
          "$gt0003|End stage renal disease|==local::at0006|No|",
          "$gt0004|Persistent acute kidney injury|==local::at0015|No|",
          "($gt0005|Serum creatinine|==local::at0019|>3x above baseline, ≥4 mg/dL (350 µmol/L), or acute rise ≥0.5 mg/dL (44 µmol/L)|)||($gt0006|GFR|==local::at0023|>75% decrease below baseline|)",
          "$gt0007|Urine output|==local::at0027|<0.3 mL/kg/hr x 24 hr (oliguria), or anuria x 12 hr|"
        ],
        "then": [
          "$gt0009| Level of renal dysfunction|=local::at0031|Failure of kidney function, RIFLE-F (RIFLE-FO if oliguria)|"
        ]
      },
      "gt0013": {
        "id": "gt0013",
        "priority": 2,
        "when": [
          "$gt0003|End stage renal disease|==local::at0006|No|",
          "$gt0004|Persistent acute kidney injury|==local::at0015|No|",
          "($gt0005|Serum creatinine|==local::at0018|>2-3x above baseline|)||($gt0006|GFR|==local::at0022|>50% decrease below baseline|)",
          "$gt0007|Urine output|==local::at0026|<0.5 mL/kg/hr x 12 hr|"
        ],
        "then": [
          "$gt0009| Level of renal dysfunction|=local::at0030|Injury to the kidney, RIFLE-I|"
        ]
      },
      "gt0014": {
        "id": "gt0014",
        "priority": 1,
        "when": [
          "$gt0003|End stage renal disease|==local::at0006|No|",
          "$gt0004|Persistent acute kidney injury|==local::at0015|No|",
          "($gt0005|Serum creatinine|==local::at0017|1.5-2x above baseline|)||($gt0006|GFR|==local::at0021|>25% decrease below baseline|)",
          "$gt0007|Urine output|==local::at0025|<0.5 mL/kg/hr x 6 hr|"
        ],
        "then": [
          "$gt0009| Level of renal dysfunction|=local::at0029|Risk of renal dysfunction, RIFLE-R|"
        ]
      }
    }
  },
