    "rules": {
      "gt0024": {
        "id": "gt0024",
        "priority": 12,
        "when": [
          "$gt0016|Gender|==null",
          "$gt0017|Age|==null",
          "$gt0018|Congestive Heart Failure|==null",
          "$gt0019|Hypertension|==null",
          "$gt0020|Stroke/TIA/Thromboembolism|==null",
          "$gt0021|Vascular disease|==null",
          "$gt0022|Diabetes|==null"
        ],
        "then": [
          "$gt0016|Gender|=0|local::at0043|Male|",
          "$gt0017|Age|=0|local::at0036|Under 65|",
          "$gt0018|Congestive Heart Failure|=0|local::at0027|Absent|",
          "$gt0019|Hypertension|=0|local::at0027|Absent|",
          "$gt0020|Stroke/TIA/Thromboembolism|=0|local::at0027|Absent|",
          "$gt0021|Vascular disease|=0|local::at0027|Absent|",
          "$gt0022|Diabetes|=0|local::at0027|Absent|"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 11,
        "when": [
          "$gt0009|Gender|==local::at0006|Female|"
        ],
        "then": [
          "$gt0016|Gender|=1|local::at0044|Female|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 10,
        "when": [
          "$gt0009|Gender|==local::at0005|Male|"
        ],
        "then": [
          "$gt0016|Gender|=0|local::at0043|Male|"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 9,
        "when": [
          "$gt0010|Birthdate|>($currentDateTime-65,a)"
        ],
        "then": [
          "$gt0017|Age|=0|local::at0036|Under 65|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 8,
        "when": [
          "$gt0010|Birthdate|<=($currentDateTime-65,a)",
          "$gt0010|Birthdate|>($currentDateTime-75,a)"
        ],
        "then": [
          "$gt0017|Age|=1|local::at0037|Between 65-74|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 7,
        "when": [
          "$gt0010|Birthdate|<=($currentDateTime-75,a)"
        ],
        "then": [
          "$gt0017|Age|=2|local::at0038|Above or equals to 75|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 6,
        "when": [
          "$gt0011|Congestive Heart Failure|!=null"
        ],
        "then": [
          "$gt0018|Congestive Heart Failure|=$gt0011|Congestive Heart Failure|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 5,
        "when": [
          "$gt0012|Hypertension|!=null"
        ],
        "then": [
          "$gt0019|Hypertension|=$gt0012|Hypertension|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 4,
        "when": [
          "$gt0013|Stroke/TIA/Thromboembolism|!=null"
        ],
        "then": [
          "$gt0020|Stroke/TIA/Thromboembolism|=$gt0013|Stroke/TIA/Thromboembolism|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 3,
        "when": [
          "$gt0014|Vascular disease|!=null"
        ],
        "then": [
          "$gt0021|Vascular disease|=$gt0014|Vascular disease|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 2,
        "when": [
          "$gt0015|Diabetes|!=null"
        ],
        "then": [
          "$gt0022|Diabetes|=$gt0015|Diabetes|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 1,
        "then": [
          "$gt0023|Total score|.magnitude=((((($gt0016.value+$gt0017.value)+$gt0018.value)+$gt0019.value)+$gt0020.value)+$gt0021.value)+$gt0022.value"
        ]
      }
