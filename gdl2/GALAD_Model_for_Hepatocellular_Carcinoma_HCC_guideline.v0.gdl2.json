        "template_id": "openEHR-EHR-OBSERVATION.galad_model_for_hepatocellular_carcinoma_hcc.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0009": {
            "id": "gt0009",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0011]"
          },
          "gt0010": {
            "id": "gt0010",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0012]"
          }
        }
      }
    },
    "rules": {
      "gt0011": {
        "id": "gt0011",
        "priority": 2,
        "when": [
          "$gt0003|Gender|!=null",
          "$gt0004|Age|!=null",
          "$gt0005|AFP-L3|!=null",
          "$gt0006|AFP|!=null",
          "$gt0007|DCP|!=null"
        ],
        "then": [
          "$gt0009|GALAD Score|.magnitude=(0.09*$gt0004.magnitude+1.67*$gt0003.value+2.34*log10($gt0006.magnitude)+0.04*log10($gt0005.magnitude)+1.33*log10($gt0007.magnitude))-10.08",
          "$gt0009|GALAD Score|.precision=2"
        ]
      },
      "gt0012": {
        "id": "gt0012",
        "priority": 1,
        "when": [
          "fired($gt0011)"
        ],
        "then": [
          "$gt0010|Probability of concurrent HCC in this CLD patient|.magnitude=(exp($gt0009.magnitude)/(1+exp($gt0009.magnitude)))*100",
          "$gt0010|Probability of concurrent HCC in this CLD patient|.unit='%'"
        ]
      }
    }
  },
