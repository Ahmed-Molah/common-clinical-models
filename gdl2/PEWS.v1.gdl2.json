    "rules": {
      "gt0015": {
        "id": "gt0015",
        "priority": 7,
        "when": [
          "$gt0009|Behaviour|==null",
          "$gt0010|Cardiovascular|==null",
          "$gt0011|Respiratory|==null",
          "$gt0012|Use of nebulizer every 15 minutes|==null",
          "$gt0013|Persisting vomiting following surgery|==null"
        ],
        "then": [
          "$gt0009|Behaviour|=0|local::at0010|Playing/appropriate|",
          "$gt0010|Cardiovascular|=0|local::at0014|Pink OR capillary refill 1-2s|",
          "$gt0011|Respiratory|=0|local::at0017|Within normal parameters|",
          "$gt0012|Use of nebulizer every 15 minutes|=0|local::at0034|No|",
          "$gt0013|Persisting vomiting following surgery|=0|local::at0036|No|"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 6,
        "when": [
          "$gt0004|Behaviour|!=null"
        ],
        "then": [
          "$gt0009|Behaviour|=$gt0004|Behaviour|"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 5,
        "when": [
          "$gt0005|Cardiovascular|!=null"
        ],
        "then": [
          "$gt0010|Cardiovascular|=$gt0005|Cardiovascular|"
        ]
      },
      "gt0018": {
        "id": "gt0018",
        "priority": 4,
        "when": [
          "$gt0006|Respiratory|!=null"
        ],
        "then": [
          "$gt0011|Respiratory|=$gt0006|Respiratory|"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 3,
        "when": [
          "$gt0007|Use of nebulizer every 15 minutes|==2|local::at0035|Yes|"
        ],
        "then": [
          "$gt0012|Use of nebulizer every 15 minutes|=2|local::at0035|Yes|"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 2,
        "when": [
          "$gt0008|Persisting vomiting following surgery|==2|local::at0037|Yes|",
          "!fired($gt0019)"
        ],
        "then": [
          "$gt0013|Persisting vomiting following surgery|=2|local::at0037|Yes|"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 1,
        "then": [
          "$gt0014|Total score|.magnitude=((($gt0009.value+$gt0010.value)+$gt0011.value)+$gt0012.value)+$gt0013.value"
        ]
      }
    }
