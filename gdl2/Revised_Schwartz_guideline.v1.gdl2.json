{
  "id": "Revised_Schwartz_guideline.v1",
  "gdl_version": "2.0",
  "concept": "gt0001",
  "language": {
    "original_language": "ISO_639-1::en"
  },
  "description": {
    "original_author": {
      "date": "2021-02-03",
      "name": " Maryam Razavi",
      "organisation": "CambioCDS",
      "email": "maryam.razavi@cambio.se"
    },
    "lifecycle_state": "Author draft",
    "details": {
      "en": {
        "id": "en",
        "purpose": "To estimate Glomerular Filtration Rate (GFR).",
        "keywords": [
          "Glomerular Filtration Rate (GFR)",
          "pediatric ",
          "creatinine"
        ],
        "use": "Use in pediatric patients with or without chronic kidney disease (CKD).  \nFormula:Estimated GFR, mL/min/1.73 m2 = k × height, cm / serum creatinine, mg/dL\n- where k = 0.413\nInterpretation: \nGFR, mL/min/1.73 m²     CKD stage     Severity\n≥90                                     1                  Normal\n60-89                                  2                  Mild\n45-59                                  3a                Mild-moderate\n30-44                                  3b                Moderate-severe\n15-29                                   4                 Severe\n<15                                       5                 End-stage\n",
        "copyright": "@CambioCDS"
      }
    },
    "other_details": {
      "references": "1. Schwartz GJ, Muñoz A, Schneider MF, Mak RH, Kaskel F, Warady BA, Furth SL. New equations to estimate GFR in children with CKD. J Am Soc Nephrol. 2009 Mar;20(3):629-37. doi: 10.1681/ASN.2008030287. Epub 2009 Jan 21. PMID: 19158356; PMCID: PMC2653687.\n2. Staples A, LeBlond R, Watkins S, Wong C, Brandt J. Validation of the revised Schwartz estimating equation in a predominantly non-CKD population. Pediatr Nephrol. 2010 Nov;25(11):2321-6. doi: 10.1007/s00467-010-1598-7. Epub 2010 Jul 22. PMID: 20652327."
    }
  },
  "definition": {
    "data_bindings": {
      "gt0002": {
        "id": "gt0002",
        "model_id": "openEHR-EHR-OBSERVATION.height.v1",
        "template_id": "openEHR-EHR-OBSERVATION.height.v1",
        "type": "INPUT",
        "elements": {
          "gt0003": {
            "id": "gt0003",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]"
          }
        }
      },
      "gt0004": {
        "id": "gt0004",
        "model_id": "openEHR-EHR-OBSERVATION.lab_test_creatinine.v1",
        "template_id": "openEHR-EHR-OBSERVATION.lab_test_creatinine.v1",
        "type": "INPUT",
        "elements": {
          "gt0005": {
            "id": "gt0005",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0005]"
          }
        }
      },
      "gt0007": {
        "id": "gt0007",
        "model_id": "openEHR-EHR-OBSERVATION.revised_schwartz_gfr.v0",
        "template_id": "openEHR-EHR-OBSERVATION.revised_schwartz_gfr.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0008": {
            "id": "gt0008",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]"
          },
          "gt0020": {
            "id": "gt0020",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0005]"
          },
          "gt0021": {
            "id": "gt0021",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0006]"
          }
        }
      },
      "gt0009": {
        "id": "gt0009",
        "model_id": "openEHR-EHR-EVALUATION.revised_schwartz_gfr.v0",
        "template_id": "openEHR-EHR-EVALUATION.revised_schwartz_gfr.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0010": {
            "id": "gt0010",
            "path": "/data[at0001]/items[at0002]"
          },
          "gt0011": {
            "id": "gt0011",
            "path": "/data[at0001]/items[at0003]"
          }
        }
      }
    },
    "rules": {
      "gt0019": {
        "id": "gt0019",
        "priority": 12,
        "when": [
          "$gt0003|Height, cm|<=25,cm"
        ],
        "then": [
          "$gt0020|Warning to Height|='Too low, please change to proceed.'"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 11,
        "when": [
          "$gt0003|Height, cm|>=509,cm"
        ],
        "then": [
          "$gt0020|Warning to Height|='Too high, please change to proceed.'"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 10,
        "when": [
          "$gt0005|Serum creatinine, mg/dL|<=0,mg/dl"
        ],
        "then": [
          "$gt0021|Warning to Creatinine|='Too low, please change to proceed.'"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 9,
        "when": [
          "$gt0005|Serum creatinine, mg/dL|>10,mg/dl",
          "$gt0005|Serum creatinine, mg/dL|<=40,mg/dl"
        ],
        "then": [
          "$gt0021|Warning to Creatinine|='Very high, double-check.'"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 8,
        "when": [
          "$gt0005|Serum creatinine, mg/dL|>40,mg/dl"
        ],
        "then": [
          "$gt0021|Warning to Creatinine|='Too high, please change to proceed.'"
        ]
      },
      "gt0012": {
        "id": "gt0012",
        "priority": 7,
        "when": [
          "$gt0003|Height, cm|!=null",
          "$gt0005|Serum creatinine, mg/dL|!=null",
          "$gt0003|Height, cm|.unit=='cm'",
          "$gt0005|Serum creatinine, mg/dL|.unit=='mg/dl'",
          "!fired($gt0019)",
          "!fired($gt0022)",
          "!fired($gt0023)",
          "!fired($gt0025)"
        ],
        "then": [
          "$gt0008|Estimated GFR|.magnitude=0.413*$gt0003.magnitude/$gt0005.magnitude",
          "$gt0008|Estimated GFR|.unit='mL/min/1.73 m2'"
        ]
      },
      "gt0013": {
        "id": "gt0013",
        "priority": 6,
        "when": [
          "$gt0008|Estimated GFR|.magnitude>=90",
          "$gt0008|Estimated GFR|.unit=='mL/min/1.73 m2'"
        ],
        "then": [
          "$gt0010|CKD stage|='1'",
          "$gt0011|Severity|='Normal'"
        ]
      },
      "gt0014": {
        "id": "gt0014",
        "priority": 5,
        "when": [
          "$gt0008|Estimated GFR|.magnitude<90",
          "$gt0008|Estimated GFR|.magnitude>59",
          "$gt0008|Estimated GFR|.unit=='mL/min/1.73 m2'"
        ],
        "then": [
          "$gt0010|CKD stage|='2'",
          "$gt0011|Severity|='Mild'"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 4,
        "when": [
          "$gt0008|Estimated GFR|.magnitude<=59",
          "$gt0008|Estimated GFR|.magnitude>44",
          "$gt0008|Estimated GFR|.unit=='mL/min/1.73 m2'"
        ],
        "then": [
          "$gt0010|CKD stage|='3a'",
          "$gt0011|Severity|='Mild-moderate'"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 3,
        "when": [
          "$gt0008|Estimated GFR|.magnitude<=44",
          "$gt0008|Estimated GFR|.magnitude>29",
          "$gt0008|Estimated GFR|.unit=='mL/min/1.73 m2'"
        ],
        "then": [
          "$gt0010|CKD stage|='3b'",
          "$gt0011|Severity|='Mild-severe'"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 2,
        "when": [
          "$gt0008|Estimated GFR|.magnitude<=29",
          "$gt0008|Estimated GFR|.magnitude>=15",
          "$gt0008|Estimated GFR|.unit=='mL/min/1.73 m2'"
        ],
        "then": [
          "$gt0010|CKD stage|='4'",
          "$gt0011|Severity|='Severe'"
        ]
      },
      "gt0018": {
        "id": "gt0018",
        "priority": 1,
        "when": [
          "$gt0008|Estimated GFR|.magnitude<15",
          "$gt0008|Estimated GFR|.unit=='mL/min/1.73 m2'"
        ],
        "then": [
          "$gt0010|CKD stage|='5'",
          "$gt0011|Severity|='End-stage renal disease'"
        ]
      }
    }
  },
