    "lifecycle_state": "Not set",
    "details": {
      "en": {
        "id": "en",
        "purpose": "Helps stratify which patients do not necessarily require observation, due to higher likelihood of aseptic (i.e., spontaneously resolving) meningitis.\nHelps avoid financial burden and health risk associated with hospitalization for observation and parenteral antibiotic administration.",
        "keywords": [
          "Bacterial",
          "Meningitis",
          "Children"
        ],
        "use": "Use in patients aged 29 days to 19 years with CSF WBC ≥10 cells/μL",
        "misuse": "Do NOT use if the patient:\n\nIs critically ill, requiring respiratory or vasopressor support.\nReceived antibiotics <72 hours prior to lumbar puncture.\nHas a VP shunt or recent neurosurgery.\nIs immunosuppressed.\nHas proof of another bacterial infection (e.g. UTI, bone infection, known bacteremia) that warrants inpatient antibiotic therapy.\nHas known active Lyme Disease."
      }
    },
    "other_details": {
      "references": "https://www.ncbi.nlm.nih.gov/pubmed/17200475\nhttps://www.ncbi.nlm.nih.gov/pubmed/22764093\nhttps://www.ncbi.nlm.nih.gov/pubmed/12359784\nhttps://www.ncbi.nlm.nih.gov/pubmed/23910166"
    }
  },
