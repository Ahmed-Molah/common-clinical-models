    "rules": {
      "gt0016": {
        "id": "gt0016",
        "priority": 14,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|"
        ],
        "then": [
          "$gt0015|Gender constant|=0,1"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 13,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|"
        ],
        "then": [
          "$gt0015|Gender constant|=1,1"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 12,
        "when": [
          "$gt0014|BMI, kg/m2|==local::at0024|<25|"
        ],
        "then": [
          "$gt0023|BMI constant|=0,1"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 11,
        "when": [
          "$gt0014|BMI, kg/m2|==local::at0025| ≥25 and <27.5|"
        ],
        "then": [
          "$gt0023|BMI constant|.magnitude=0.699"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 10,
        "when": [
          "$gt0014|BMI, kg/m2|==local::at0026|≥27.5 and <30|"
        ],
        "then": [
          "$gt0023|BMI constant|=1.97,1"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 9,
        "when": [
          "$gt0014|BMI, kg/m2|==local::at0027|≥30|"
        ],
        "then": [
          "$gt0023|BMI constant|=2.518,1"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 8,
        "when": [
          "$gt0006|Family history|==local::at0029|No diabetic 1st-degree relative|"
        ],
        "then": [
          "$gt0030|Family history constant|=0,1"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 7,
        "when": [
          "$gt0006|Family history|==local::at0030|Parent or sibling with diabetes|"
        ],
        "then": [
          "$gt0030|Family history constant|=0.728,1"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 6,
        "when": [
          "$gt0006|Family history|==local::at0031|Parent and sibling with diabetes|"
        ],
        "then": [
          "$gt0030|Family history constant|=0.753,1"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 5,
        "when": [
          "$gt0009|Smoking history|==local::at0033|Non-smoker|"
        ],
        "then": [
          "$gt0029|Smoking history constant|=0,1"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 4,
        "when": [
          "$gt0009|Smoking history|==local::at0035|Current smoker|"
        ],
        "then": [
          "$gt0029|Smoking history constant|=0.855,1"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 3,
        "when": [
          "$gt0009|Smoking history|==local::at0034|Ex-smoker|"
        ],
        "then": [
          "$gt0029|Smoking history constant|=2,1"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 2,
        "then": [
          "$gt0038|Exponential expression|.magnitude=$gt0015.magnitude+($gt0007.value*1.222)+($gt0004.magnitude*0.063)+$gt0023.magnitude+$gt0030.magnitude+$gt0029.magnitude",
          "$gt0038|Exponential expression|.precision=2"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 1,
        "when": [
          "$gt0038|Exponential expression|!=null"
        ],
        "then": [
          "$gt0022|Risk Score|.magnitude=1/(1+(e^$gt0038.magnitude))",
          "$gt0022|Risk Score|.precision=2"
        ]
      }
    }
  },
