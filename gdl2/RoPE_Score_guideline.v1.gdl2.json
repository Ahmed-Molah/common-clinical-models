        "id": "gt0011",
        "model_id": "openEHR-EHR-EVALUATION.rope_score.v0",
        "template_id": "openEHR-EHR-EVALUATION.rope_score.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0012": {
            "id": "gt0012",
