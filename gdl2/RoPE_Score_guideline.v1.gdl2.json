          },
          "gt0013": {
            "id": "gt0013",
            "path": "/data[at0001]/items[at0003]"
          }
        }
      }
    },
    "rules": {
      "gt0014": {
        "id": "gt0014",
        "priority": 8,
        "when": [
          "$gt0003|Age in years|!=null",
          "$gt0004|History of hypertension|!=null",
          "$gt0005|History of diabetes|!=null",
          "$gt0006|History of stroke or TIA|!=null",
          "$gt0007|Smoker|!=null",
          "$gt0008|Cortical infarct on imaging|!=null"
        ],
        "then": [
          "$gt0010|RoPE Score|.magnitude=$gt0003.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 7,
        "when": [
          "$gt0010|RoPE Score|>=0",
          "$gt0010|RoPE Score|<4"
        ],
        "then": [
          "$gt0012|PFO-attributable fraction (95% CI)|=local::at0004|0% (0–4)|",
          "$gt0013|Estimated stroke/TIA recurrence at 2 years|=local::at0011|20% (12–28)|"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 6,
        "when": [
          "$gt0010|RoPE Score|==4"
        ],
        "then": [
          "$gt0012|PFO-attributable fraction (95% CI)|=local::at0005|38% (25–48)|",
          "$gt0013|Estimated stroke/TIA recurrence at 2 years|=local::at0012|12% (6–18)|"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 5,
        "when": [
          "$gt0010|RoPE Score|==5"
        ],
        "then": [
          "$gt0012|PFO-attributable fraction (95% CI)|=local::at0006|34% (21–45)|",
          "$gt0013|Estimated stroke/TIA recurrence at 2 years|=local::at0013|7% (3–11)|"
        ]
      },
      "gt0018": {
        "id": "gt0018",
        "priority": 4,
        "when": [
          "$gt0010|RoPE Score|==6"
        ],
        "then": [
          "$gt0012|PFO-attributable fraction (95% CI)|=local::at0007|62% (54–68)|",
          "$gt0013|Estimated stroke/TIA recurrence at 2 years|=local::at0014|8% (4–12)|"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 3,
        "when": [
          "$gt0010|RoPE Score|==7"
        ],
        "then": [
          "$gt0012|PFO-attributable fraction (95% CI)|=local::at0008|72% (66–76)|",
          "$gt0013|Estimated stroke/TIA recurrence at 2 years|=local::at0015|6% (2–10)|"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 2,
        "when": [
          "$gt0010|RoPE Score|==8"
        ],
        "then": [
          "$gt0012|PFO-attributable fraction (95% CI)|=local::at0009|84% (79–87)|",
          "$gt0013|Estimated stroke/TIA recurrence at 2 years|=local::at0015|6% (2–10)|"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 1,
        "when": [
          "$gt0010|RoPE Score|>8",
          "$gt0010|RoPE Score|<=10"
        ],
        "then": [
          "$gt0012|PFO-attributable fraction (95% CI)|=local::at0010|88% (83–91)|",
          "$gt0013|Estimated stroke/TIA recurrence at 2 years|=local::at0017|2% (0–4)|"
        ]
      }
    }
  },
