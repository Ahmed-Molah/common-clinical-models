    "rules": {
      "gt0018": {
        "id": "gt0018",
        "priority": 6,
        "when": [
          "$gt0003|Sodium|.unit=='mmol/l'",
          "$gt0005|BUN|.unit=='mmol/L'",
          "$gt0007|Glucose|.unit=='mmol/l'",
          "$gt0009|Serum alcohol concentration|.unit=='mmol/L'"
        ],
        "then": [
          "$gt0013|Serum osmolality|.unit='mOsm/kg'",
          "$gt0013|Serum osmolality|.magnitude=$gt0003.magnitude*2+$gt0005.magnitude+$gt0007.magnitude+$gt0009.magnitude"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 5,
        "when": [
          "$gt0003|Sodium|.unit=='mEq/l'",
          "$gt0005|BUN|.unit=='mg/dL'",
          "$gt0007|Glucose|.unit=='mg/dl'",
          "$gt0009|Serum alcohol concentration|.unit=='mg/dL'"
        ],
        "then": [
          "$gt0013|Serum osmolality|.unit='mOsm/kg'",
          "$gt0013|Serum osmolality|.magnitude=$gt0003.magnitude*2+($gt0005.magnitude/2.8)+($gt0007.magnitude/18)+($gt0009.magnitude/3.7)"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 4,
        "when": [
          "$gt0013|Serum osmolality|>=285,mOsm/kg",
          "$gt0013|Serum osmolality|<=295,mOsm/kg"
        ],
        "then": [
          "$gt0016|Serum osmolality interpretation|.value=$gt0023.term"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 3,
        "when": [
          "$gt0013|Serum osmolality|!=null",
          "$gt0011|Measured serum osm|!=null"
        ],
        "then": [
          "$gt0014|Osmolal gap|.unit='mOsm/kg'",
          "$gt0014|Osmolal gap|.magnitude=$gt0011.magnitude-$gt0013.magnitude",
          "$gt0014|Osmolal gap|.precision=1"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 1,
        "when": [
          "$gt0014|Osmolal gap|<=10,mOsm/kg"
        ],
        "then": [
          "$gt0017|Osmolal gap interpretation|.value=$gt0024.term"
        ]
      }
    }
  },
