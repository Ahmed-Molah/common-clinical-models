{
  "id": "PSADT_guideline.v1",
  "gdl_version": "2.0",
  "concept": "gt0001",
  "language": {
    "original_language": "ISO_639-1::en"
  },
  "description": {
    "original_author": {
      "date": "2020-12-10",
      "name": "Maryam Razavi",
      "organisation": "CambioCDS",
      "email": "maryam.razavi@cambio.se"
    },
    "lifecycle_state": "Not set",
    "details": {
      "en": {
        "id": "en",
        "purpose": "To calculate rate of PSA doubling in prostate cancer (correlates with survival).",
        "keywords": [
          "PSA:prostate specific antigen",
          "prostate",
          "cancer",
          "prostatectomy "
        ],
        "use": "Used in patients with prostate cancer who have undergone radical prostatectomy or definitive radiation therapy and have biochemical recurrence, defined as confirmed PSA ≥0.2 ng/mL if radical prostatectomy or confirmed PSA rise of ≥2 ng/mL above the nadir PSA level if radiation therapy.\n\nVariables: \nPSA1 (First (earliest) available PSA result) ng/mL\nPSA2  (Second available PSA result) ng/mL\nTime between PSA1 and PSA2 measurement month\nFormula: \nPSA doubling time = ln (2) / PSA Slope\nPSA Slope = (ln (PSA level 2) – ln (PSA level 1)) / Time (no. of months)",
        "copyright": "CambioCDS"
      }
    },
    "other_details": {
      "references": "1. Pound CR, Partin AW, Eisenberger MA, Chan DW, Pearson JD, Walsh PC. Natural history of progression after PSA elevation following radical prostatectomy. JAMA. 1999;281(17):1591-1597.\n2. Vickers AJ, Brewster SF. PSA Velocity and Doubling Time in Diagnosis and Prognosis of Prostate Cancer. Br J Med Surg Urol. 2012; 5(4):162-168.\n3. Ponholzer A, Popper N, Breitenecker F, et al. Proposal for a standardized PSA doubling-time calculation. Anticancer Res. 2010; 30(5):1633-1636."
    }
  },
  "definition": {
    "data_bindings": {
      "gt0002": {
        "id": "gt0002",
        "model_id": "openEHR-EHR-OBSERVATION.lab_test_psa.v1",
        "template_id": "openEHR-EHR-OBSERVATION.lab_test_psa.v1",
        "type": "INPUT",
        "elements": {
          "gt0003": {
            "id": "gt0003",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]"
          }
        }
      },
      "gt0006": {
        "id": "gt0006",
        "model_id": "openEHR-EHR-OBSERVATION.psa_doubling_time.v0",
        "template_id": "openEHR-EHR-OBSERVATION.psa_doubling_time.v0",
        "type": "INPUT",
        "elements": {
          "gt0013": {
            "id": "gt0013",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0011]"
          },
          "gt0014": {
            "id": "gt0014",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0007]"
          }
        }
      },
      "gt0008": {
        "id": "gt0008",
        "model_id": "openEHR-EHR-OBSERVATION.psa_doubling_time.v0",
        "template_id": "openEHR-EHR-OBSERVATION.psa_doubling_time.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0009": {
            "id": "gt0009",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0008]"
          },
          "gt0010": {
            "id": "gt0010",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0009]"
          },
          "gt0016": {
            "id": "gt0016",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0012]"
          }
        }
      }
    },
    "rules": {
      "gt0017": {
        "id": "gt0017",
        "priority": 3,
        "when": [
          "$gt0013|PSA2|.magnitude<=$gt0003.magnitude"
        ],
        "then": [
          "$gt0016|Warning|='PSA value should be increasing over time'"
        ]
      },
      "gt0012": {
        "id": "gt0012",
        "priority": 2,
        "when": [
          "$gt0003|PSA1|!=null",
          "$gt0013|PSA2|!=null",
          "$gt0014|Time between PSA1 and PSA2 measurement|!=null",
          "$gt0014|Time between PSA1 and PSA2 measurement|.unit=='mo'",
          "!fired($gt0017)"
        ],
        "then": [
          "$gt0010|Slope PSA|.magnitude=(log($gt0013.magnitude)-log($gt0003.magnitude))/$gt0014.magnitude",
          "$gt0010|Slope PSA|.precision=1",
          "$gt0010|Slope PSA|.unit='1'"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 1,
        "when": [
          "$gt0010|Slope PSA|!=null"
        ],
        "then": [
          "$gt0009|PSA doubling time |.magnitude=log(2)/$gt0010.magnitude",
          "$gt0009|PSA doubling time |.precision=1",
          "$gt0009|PSA doubling time |.unit='mo'"
        ]
      }
    }
  },
