      "gt0082": {
        "id": "gt0082",
        "priority": 29,
        "when": [
          "$gt0037|Gender|==local::at0006|Female|",
          "$gt0081|Age|>=3,a",
          "$gt0081|Age|<=7,a"
        ],
        "then": [
          "$gt0078|Age Value|=0.381,1"
        ]
      },
      "gt0083": {
        "id": "gt0083",
        "priority": 28,
        "when": [
          "$gt0037|Gender|==local::at0006|Female|",
          "$gt0081|Age|>=8,a",
          "$gt0081|Age|<=11,a"
        ],
        "then": [
          "$gt0078|Age Value|=0.6513,1"
        ]
      },
      "gt0084": {
        "id": "gt0084",
        "priority": 27,
        "when": [
          "$gt0037|Gender|==local::at0005|Male|",
          "$gt0081|Age|>=3,a",
          "$gt0081|Age|<=7,a"
        ],
        "then": [
          "$gt0078|Age Value|.magnitude='-0.6653'",
          "$gt0078|Age Value|.precision=4"
        ]
      },
      "gt0085": {
        "id": "gt0085",
        "priority": 26,
        "when": [
          "$gt0037|Gender|==local::at0005|Male|",
          "$gt0081|Age|>=8,a",
          "$gt0081|Age|<=13,a"
        ],
        "then": [
          "$gt0078|Age Value|.magnitude='-0.0654'",
          "$gt0078|Age Value|.precision=4"
        ]
      },
      "gt0087": {
        "id": "gt0087",
        "priority": 25,
        "when": [
          "$gt0072|Duration of pain, hours|==local::at0017|24 to <48|"
        ],
        "then": [
          "$gt0053|Duration of pain Value|=0.4696,1"
        ]
      },
      "gt0088": {
        "id": "gt0088",
        "priority": 24,
        "when": [
          "$gt0072|Duration of pain, hours|==local::at0018|48 to 96|"
        ],
        "then": [
          "$gt0053|Duration of pain Value|=0.1003,1"
        ]
      },
      "gt0092": {
        "id": "gt0092",
        "priority": 23,
        "when": [
          "$gt0073|Presence of pain with walking|==local::at0024|Yes|"
        ],
        "then": [
          "$gt0054|Presence of pain with walking Value|=1.0494,1"
        ]
      },
      "gt0093": {
        "id": "gt0093",
        "priority": 22,
        "when": [
          "$gt0074|Maximal tenderness in RLQ|==local::at0027|Yes|"
        ],
        "then": [
          "$gt0055|Maximum tenderness in RLQ Value|=1.1435,1"
        ]
      },
      "gt0094": {
        "id": "gt0094",
        "priority": 21,
        "when": [
          "$gt0075|Abdominal guarding|==local::at0030|Yes|"
        ],
        "then": [
          "$gt0091|Abdominal guarding Value|=0.6736,1"
        ]
      },
      "gt0095": {
        "id": "gt0095",
        "priority": 20,
        "when": [
          "$gt0076|History of migration of pain to RLQ|==local::at0033|Yes|"
        ],
        "then": [
          "$gt0056|History of migration of pain to RLQ Value|=0.4557,1"
        ]
      },
      "gt0096": {
        "id": "gt0096",
        "priority": 19,
        "when": [
          "$gt0047|Neutrophil, % (Enter a value for neutrophil if it is available; otherwise, leave it as blank)|!=null",
          "$gt0046|White cell count, 10^3/uL|!=null"
        ],
        "then": [
          "$gt0050|ANC, cells x 10^3/µL|.magnitude=($gt0047.magnitude*$gt0046.magnitude)/100",
          "$gt0050|ANC, cells x 10^3/µL|.unit='10^3/uL'",
          "$gt0050|ANC, cells x 10^3/µL|.precision=4"
        ]
      },
      "gt0097": {
        "id": "gt0097",
        "priority": 18,
        "when": [
          "$gt0047|Neutrophil, % (Enter a value for neutrophil if it is available; otherwise, leave it as blank)|==null",
          "$gt0046|White cell count, 10^3/uL|!=null"
        ],
        "then": [
          "$gt0050|ANC, cells x 10^3/µL|.magnitude=(1.1008*sqrt($gt0046.magnitude)-0.8783)^2",
          "$gt0050|ANC, cells x 10^3/µL|.unit='10^3/uL'",
          "$gt0050|ANC, cells x 10^3/µL|.precision=4"
        ]
      },
      "gt0098": {
        "id": "gt0098",
        "priority": 17,
        "when": [
          "$gt0050|ANC, cells x 10^3/µL|.magnitude<14",
          "$gt0050|ANC, cells x 10^3/µL|.unit=='10^3/uL'"
        ],
        "then": [
          "$gt0057|ANC Value|.magnitude=1.7734*sqrt($gt0050.magnitude)",
          "$gt0057|ANC Value|.precision=4",
          "$gt0057|ANC Value|.unit='1'"
        ]
      },
      "gt0099": {
        "id": "gt0099",
        "priority": 16,
        "when": [
          "$gt0050|ANC, cells x 10^3/µL|.magnitude>=14",
          "$gt0050|ANC, cells x 10^3/µL|.unit=='10^3/uL'"
        ],
        "then": [
          "$gt0057|ANC Value|=6.6195,1"
        ]
      },
      "gt0100": {
        "id": "gt0100",
        "priority": 15,
        "when": [
          "$gt0081|Age|!=null",
          "$gt0037|Gender|!=null",
          "$gt0072|Duration of pain, hours|!=null",
          "$gt0073|Presence of pain with walking|!=null",
          "$gt0074|Maximal tenderness in RLQ|!=null",
          "$gt0075|Abdominal guarding|!=null",
          "$gt0076|History of migration of pain to RLQ|!=null"
        ],
        "then": [
          "$gt0101|x|=$gt0051.magnitude+$gt0053.magnitude+$gt0054.magnitude+$gt0055.magnitude+$gt0056.magnitude+$gt0057.magnitude+$gt0078.magnitude+$gt0091.magnitude-8.6855"
        ]
      },
      "gt0102": {
        "id": "gt0102",
        "priority": 14,
        "when": [
          "$gt0071|Setting|==local::at0007|Pediatric ED|"
        ],
        "then": [
          "$gt0058|pARC Score (ED)|.magnitude=floor((2.71828^$gt0101)/(1+(2.71828^$gt0101))*100)",
          "$gt0058|pARC Score (ED)|.unit='%'"
        ]
      },
      "gt0103": {
        "id": "gt0103",
        "priority": 13,
        "when": [
          "$gt0071|Setting|==local::at0006|Community|"
        ],
        "then": [
          "$gt0109|pARC Score (Community)|.magnitude=floor((2.71828^(1.1*$gt0101-0.615))/(1+(2.71828^(1.1*$gt0101-0.615)))*100)",
          "$gt0109|pARC Score (Community)|.unit='%'"
        ]
      },
      "gt0105": {
        "id": "gt0105",
        "priority": 12,
        "when": [
          "$gt0058|pARC Score (ED)|<=5,%"
        ],
        "then": [
          "$gt0060|Pediatric ED Setting Risk Group|=local::at0003|Ultra low Risk|",
          "$gt0061|Clinical Recommendations in Pediatric ED Setting |=local::at0010|Outpatient follow up appropriate if PCP evaluation available within 24 hrs, no diagnostic imaging required|"
        ]
      },
      "gt0106": {
        "id": "gt0106",
        "priority": 11,
        "when": [
          "$gt0058|pARC Score (ED)|>=6,%",
          "$gt0058|pARC Score (ED)|<=15,%"
        ],
        "then": [
          "$gt0060|Pediatric ED Setting Risk Group|=local::at0004|Low Risk|",
          "$gt0061|Clinical Recommendations in Pediatric ED Setting |=local::at0011|Consider ED observation for 6 hrs for serial exams. If improved, ensure outpatient follow-up in 24 hrs, no imaging required.|"
        ]
      },
      "gt0107": {
        "id": "gt0107",
        "priority": 10,
        "when": [
          "$gt0058|pARC Score (ED)|>=16,%",
          "$gt0058|pARC Score (ED)|<=25,%"
        ],
        "then": [
          "$gt0060|Pediatric ED Setting Risk Group|=local::at0005|Low-moderate Risk|",
          "$gt0061|Clinical Recommendations in Pediatric ED Setting |=local::at0012|If patients symptoms <24 hrs, consider observation for 12 hrs. If not improved, obtain ultrasound and repeat CBC. If pain ≥24 hrs of symptoms, obtain ultrasound to evaluate for appendicitis|"
        ]
      },
      "gt0108": {
        "id": "gt0108",
        "priority": 9,
        "when": [
          "$gt0058|pARC Score (ED)|>=26,%",
          "$gt0058|pARC Score (ED)|<=50,%"
        ],
        "then": [
          "$gt0060|Pediatric ED Setting Risk Group|=local::at0006|Moderate Risk|",
          "$gt0061|Clinical Recommendations in Pediatric ED Setting |=local::at0013|Ultrasound recommended as first line imaging. Admit for observation if ultrasound equivocal|"
        ]
      },
      "gt0110": {
        "id": "gt0110",
        "priority": 8,
        "when": [
          "$gt0058|pARC Score (ED)|>=51,%",
          "$gt0058|pARC Score (ED)|<=75,%"
        ],
        "then": [
          "$gt0060|Pediatric ED Setting Risk Group|=local::at0006|Moderate Risk|",
          "$gt0061|Clinical Recommendations in Pediatric ED Setting |=local::at0014|Ultrasound recommended as first line imaging, CT if ultrasound equivocal|"
        ]
      },
      "gt0111": {
        "id": "gt0111",
        "priority": 7,
        "when": [
          "$gt0058|pARC Score (ED)|>=76,%",
          "$gt0058|pARC Score (ED)|<=90,%"
        ],
        "then": [
          "$gt0060|Pediatric ED Setting Risk Group|=local::at0007|Moderate-high Risk|",
          "$gt0061|Clinical Recommendations in Pediatric ED Setting |=local::at0015|Consult surgery. Consider imaging based on surgery recommendations|"
        ]
      },
      "gt0112": {
        "id": "gt0112",
        "priority": 6,
        "when": [
          "$gt0058|pARC Score (ED)|>90,%"
        ],
        "then": [
          "$gt0060|Pediatric ED Setting Risk Group|=local::at0008|High Risk|",
          "$gt0061|Clinical Recommendations in Pediatric ED Setting |=local::at0016|Consult surgery, imaging not required|"
        ]
      },
      "gt0113": {
        "id": "gt0113",
        "priority": 5,
        "when": [
          "$gt0109|pARC Score (Community)|<5,%"
        ],
        "then": [
          "$gt0062|Community Setting Risk Group|=local::at0018|Very Low Risk|"
        ]
      },
      "gt0114": {
        "id": "gt0114",
        "priority": 4,
        "when": [
          "$gt0109|pARC Score (Community)|>=5,%",
          "$gt0109|pARC Score (Community)|<=14,%"
        ],
        "then": [
          "$gt0062|Community Setting Risk Group|=local::at0019|Low Risk|"
        ]
      },
      "gt0115": {
        "id": "gt0115",
        "priority": 3,
        "when": [
          "$gt0109|pARC Score (Community)|>=15,%",
          "$gt0109|pARC Score (Community)|<=84,%"
        ],
        "then": [
          "$gt0062|Community Setting Risk Group|=local::at0020|Intermediate Risk|"
        ]
      },
      "gt0117": {
        "id": "gt0117",
        "priority": 1,
        "when": [
          "$gt0109|pARC Score (Community)|>=85,%"
        ],
        "then": [
          "$gt0062|Community Setting Risk Group|=local::at0021|High Risk|"
        ]
      }
    }
  },
