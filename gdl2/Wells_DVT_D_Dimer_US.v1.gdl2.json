      "en": {
        "id": "en",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "Wells’ Criteria for DVT (D-Dimer)",
            "description": "The Wells’ Criteria for DVT are used for evaluating the pre-test probability for deep vein thrombosis (DVT). Two points or greater indicates high probability of DVT."
          },
          "gt0003": {
            "id": "gt0003",
            "text": "Pitting oedema ",
            "description": "Greater in the symptomatic leg."
          },
          "gt0004": {
            "id": "gt0004",
            "text": "Paralysis, paresis, or recent plaster immobilisation of the legs",
            "description": "*"
          },
          "gt0005": {
            "id": "gt0005",
            "text": "Active cancer (treatment ongoing or within previous 6 months or palliative)",
            "description": "*"
          },
          "gt0006": {
            "id": "gt0006",
            "text": "Recently bedridden for more than 3 days or major surgery within 4 weeks",
            "description": "*"
          },
          "gt0007": {
            "id": "gt0007",
            "text": "Localised tenderness along the distribution of the deep venous system",
            "description": "*"
          },
          "gt0008": {
            "id": "gt0008",
            "text": "Entire leg swollen",
            "description": "*"
          },
          "gt0009": {
            "id": "gt0009",
            "text": "Calf swelling by more than 3 cm compared with the asymptomatic leg",
            "description": "Measured 10 cm below the tibial tuberosity."
          },
          "gt0010": {
            "id": "gt0010",
            "text": "Collateral superficial veins (non-varicose)",
