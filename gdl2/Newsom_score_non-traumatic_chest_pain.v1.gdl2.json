    "rules": {
      "gt0029": {
        "id": "gt0029",
        "priority": 11,
        "when": [
          "$gt0003|Age|>=60,a"
        ],
        "then": [
          "$gt0021|Age ≥ 60 years|=1|local::at0006|Yes|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 10,
        "when": [
          "$gt0003|Age|<60,a"
        ],
        "then": [
          "$gt0021|Age ≥ 60 years|=0|local::at0005|No|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 9,
        "when": [
          "(($gt0012|Body Temperature|.magnitude>=38)&&($gt0012|Body Temperature|.unit=='°C'))||(($gt0012|Body Temperature|.magnitude>=100.4)&&($gt0012|Body Temperature|.unit=='°F'))"
        ],
        "then": [
          "$gt0022|Fever ≥ 100.4°F (38°C)|=1|local::at0027|Yes|"
        ]
      },
      "gt0041": {
        "id": "gt0041",
        "priority": 8,
        "when": [
          "(($gt0012|Body Temperature|.magnitude<38)&&($gt0012|Body Temperature|.unit=='°C'))||(($gt0012|Body Temperature|.magnitude<100.4)&&($gt0012|Body Temperature|.unit=='°F'))"
        ],
        "then": [
          "$gt0022|Fever ≥ 100.4°F (38°C)|=0|local::at0026|No|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 7,
        "when": [
          "$gt0014|Respiration rate|>24,/min"
        ],
        "then": [
          "$gt0024|Respiratory rate > 24 per minute|=1|local::at0033|Yes|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 6,
        "when": [
          "$gt0014|Respiration rate|<=24,/min"
        ],
        "then": [
          "$gt0024|Respiratory rate > 24 per minute|=0|local::at0032|No|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 5,
        "when": [
          "$gt0016|SpO₂|<90,100,2"
        ],
        "then": [
          "$gt0023|Oxygen saturation < 90%|=1|local::at0030|Yes|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 4,
        "when": [
          "$gt0016|SpO₂|>=90,100,2"
        ],
        "then": [
          "$gt0023|Oxygen saturation < 90%|=0|local::at0029|No|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 3,
        "when": [
          "$gt0021|Age ≥ 60 years|!=null",
          "$gt0005|CHF history|!=null",
          "$gt0006|Smoking history|!=null",
          "$gt0007|Hemoptysis|!=null",
          "$gt0008|Tuberculosis history|!=null",
          "$gt0009|Thromboembolic history|!=null",
          "$gt0010|Prior/current alcohol abuse|!=null",
          "$gt0022|Fever ≥ 100.4°F (38°C)|!=null",
          "$gt0023|Oxygen saturation < 90%|!=null",
          "$gt0024|Respiratory rate > 24 per minute|!=null",
          "$gt0018|Diminished breath sounds|!=null",
          "$gt0019|Rales|!=null"
        ],
        "then": [
          "$gt0025|Total score|.magnitude=$gt0021.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0022.value+$gt0023.value+$gt0024.value+$gt0018.value+$gt0019.value"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 2,
        "when": [
          "$gt0025|Total score|<1"
        ],
        "then": [
          "$gt0027|Risk|=0|local::at0003|Low risk|",
          "$gt0028|Recommendation|=0|local::at0006|Chest x-ray not necessary|"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 1,
        "when": [
          "$gt0025|Total score|>=1"
        ],
        "then": [
          "$gt0027|Risk|=1|local::at0004|Not low risk|",
          "$gt0028|Recommendation|=1|local::at0007|Consider chest x-ray|"
        ]
      }
    }
