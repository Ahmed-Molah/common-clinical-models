    "rules": {
      "gt0043": {
        "id": "gt0043",
        "priority": 14,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=10,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<15,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=-35|local::at0014|10-14|"
        ]
      },
      "gt0044": {
        "id": "gt0044",
        "priority": 13,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=15,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<20,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=-30|local::at0013|15-19|"
        ]
      },
      "gt0045": {
        "id": "gt0045",
        "priority": 12,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=20,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<25,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=-25|local::at0012|20-24|"
        ]
      },
      "gt0046": {
