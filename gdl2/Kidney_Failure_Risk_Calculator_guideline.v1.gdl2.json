        ]
      },
      "gt0048": {
        "id": "gt0048",
        "priority": 9,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=35,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<39,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=-10|local::at0009|35-39|"
        ]
      },
      "gt0049": {
        "id": "gt0049",
        "priority": 8,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=40,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<45,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=-5|local::at0008|40-44|"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 7,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=45,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<50,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=0|local::at0005|45-49|"
        ]
      },
      "gt0051": {
        "id": "gt0051",
        "priority": 6,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=50,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<55,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=5|local::at0006|50-54|"
        ]
      },
      "gt0052": {
        "id": "gt0052",
        "priority": 5,
        "when": [
          "$gt0004|eGFR, mL/min/1.73m²|>=55,ml/min",
          "$gt0004|eGFR, mL/min/1.73m²|<=59,ml/min"
        ],
        "then": [
          "$gt0031|eGFR, mL/min/1.73m²|=10|local::at0007|55-59|"
        ]
      },
      "gt0053": {
        "id": "gt0053",
        "priority": 4,
        "when": [
          "$gt0018|Urine albumin|!=null",
          "$gt0020|Urine creatinine|!=null",
          "$gt0018|Urine albumin|.unit=='mg/dl'",
          "$gt0020|Urine creatinine|.unit=='gm/dl'"
        ],
        "then": [
          "$gt0042|Urine albumin-to-creatinine ratio, mg/g|.magnitude=round($gt0018.magnitude/$gt0020.magnitude)",
          "$gt0042|Urine albumin-to-creatinine ratio, mg/g|.unit='mg/gm'"
        ]
      },
      "gt0054": {
        "id": "gt0054",
        "priority": 3,
        "when": [
          "$gt0042|Urine albumin-to-creatinine ratio, mg/g|<30,mg/gm"
        ],
        "then": [
          "$gt0032|Urine albumin-to-creatinine ratio, mg/g|=0|local::at0021|<30|"
        ]
      },
      "gt0055": {
        "id": "gt0055",
        "priority": 2,
        "when": [
          "$gt0042|Urine albumin-to-creatinine ratio, mg/g|>=30,mg/gm",
          "$gt0042|Urine albumin-to-creatinine ratio, mg/g|<=300,mg/gm"
        ],
        "then": [
          "$gt0032|Urine albumin-to-creatinine ratio, mg/g|=-14|local::at0020|30-300|"
        ]
      },
      "gt0056": {
        "id": "gt0056",
        "priority": 1,
        "when": [
          "$gt0042|Urine albumin-to-creatinine ratio, mg/g|>300,mg/gm"
        ],
        "then": [
          "$gt0032|Urine albumin-to-creatinine ratio, mg/g|=-22|local::at0019|>300|"
        ]
      }
    }
