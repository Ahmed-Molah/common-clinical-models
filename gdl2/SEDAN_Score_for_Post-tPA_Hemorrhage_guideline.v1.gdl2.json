  "definition": {
    "data_bindings": {
      "gt0002": {
        "id": "gt0002",
        "model_id": "openEHR-EHR-OBSERVATION.basic_demographic.v1",
        "template_id": "openEHR-EHR-OBSERVATION.basic_demographic.v1",
        "type": "INPUT",
        "elements": {
          "gt0003": {
            "id": "gt0003",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0013]"
          }
        }
      },
      "gt0004": {
        "id": "gt0004",
        "model_id": "openEHR-EHR-OBSERVATION.sedan_score_for_post_tpa_hemorrhage.v0",
        "template_id": "openEHR-EHR-OBSERVATION.sedan_score_for_post_tpa_hemorrhage.v0",
        "type": "INPUT",
        "elements": {
          "gt0005": {
            "id": "gt0005",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]"
          },
          "gt0008": {
            "id": "gt0008",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0009]"
          },
          "gt0009": {
            "id": "gt0009",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0012]"
          },
          "gt0011": {
            "id": "gt0011",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0021]"
          }
        }
      },
      "gt0012": {
        "id": "gt0012",
        "model_id": "openEHR-EHR-OBSERVATION.sedan_score_for_post_tpa_hemorrhage.v0",
        "template_id": "openEHR-EHR-OBSERVATION.sedan_score_for_post_tpa_hemorrhage.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0013": {
            "id": "gt0013",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0015]"
          },
          "gt0014": {
            "id": "gt0014",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0005]"
          },
          "gt0015": {
            "id": "gt0015",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0018]"
          },
          "gt0016": {
            "id": "gt0016",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0022]"
          }
        }
      }
    },
    "rules": {
      "gt0017": {
        "id": "gt0017",
        "priority": 8,
        "when": [
          "($gt0005|Blood sugar|<=144,mg/dl)||($gt0005|Blood sugar|<=8,mmol/l)"
        ],
        "then": [
          "$gt0014|Blood sugar|=0|local::at0006|<145 mg/dL (8.1 mmol/L)|"
        ]
      },
      "gt0018": {
        "id": "gt0018",
        "priority": 7,
        "when": [
          "(($gt0005|Blood sugar|>144,mg/dl)&&($gt0005|Blood sugar|<=216,mg/dl))||(($gt0005|Blood sugar|>8,mmol/l)&&($gt0005|Blood sugar|<=12,mmol/l))"
        ],
        "then": [
          "$gt0014|Blood sugar|=1|local::at0007|145-216 mg/dL (8.1–12.0 mmol/L)|"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 6,
        "when": [
          "($gt0005|Blood sugar|>216,mg/dl)||($gt0005|Blood sugar|>12,mmol/l)"
        ],
        "then": [
          "$gt0014|Blood sugar|=2|local::at0008|>216 mg/dL (12.0 mmol/L)|"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 5,
        "when": [
          "$gt0003|Age|<=75,a"
        ],
        "then": [
          "$gt0013|Age > 75 years|=0|local::at0016|No|"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 4,
        "when": [
          "$gt0003|Age|>75,a"
        ],
        "then": [
          "$gt0013|Age > 75 years|=1|local::at0017|Yes|"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 3,
        "when": [
          "$gt0011|NIH Stroke Scale|>=0,Points",
          "$gt0011|NIH Stroke Scale|<10,Points"
        ],
        "then": [
          "$gt0015|NIH Stroke Scale (NIHSS) ≥ 10|=0|local::at0019|No|"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 2,
        "when": [
          "$gt0011|NIH Stroke Scale|>=10,Points"
        ],
        "then": [
          "$gt0015|NIH Stroke Scale (NIHSS) ≥ 10|=1|local::at0020|Yes|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 1,
        "when": [
          "$gt0003|Age|!=null",
          "$gt0005|Blood sugar|!=null",
          "$gt0008|Early infarct signs on initial CT|!=null",
          "$gt0009|Hyperdense cerebral artery sign on initial CT|!=null",
          "$gt0011|NIH Stroke Scale|!=null"
        ],
        "then": [
          "$gt0016|SEDAN Score|.magnitude=$gt0008.value+$gt0009.value+$gt0013.value+$gt0015.value+$gt0014.value"
        ]
      }
    }
  },
  "ontology": {
    "term_definitions": {
      "en": {
        "id": "en",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "SEDAN Score for Post-tPA Hemorrhage"
          },
          "gt0003": {
            "id": "gt0003",
            "text": "Age",
            "description": "Age in years, and for babies: months, weeks or days"
          },
          "gt0005": {
            "id": "gt0005",
            "text": "Blood sugar",
            "description": "*"
          },
          "gt0008": {
            "id": "gt0008",
            "text": "Early infarct signs on initial CT",
            "description": "Hypoattenuation comprising <1/3 of the middle cerebral artery territory; obscuration of the lentiform nucleus or Sylvian fissure; loss of basal ganglion outline or insular ribbon; cortical sulcal effacement."
          },
          "gt0009": {
            "id": "gt0009",
            "text": "Hyperdense cerebral artery sign on initial CT",
            "description": "*"
          },
          "gt0011": {
            "id": "gt0011",
            "text": "NIH Stroke Scale",
            "description": "*"
          },
          "gt0013": {
            "id": "gt0013",
            "text": "Age > 75 years",
            "description": "*"
          },
          "gt0014": {
            "id": "gt0014",
            "text": "Blood sugar",
            "description": "*"
          },
          "gt0015": {
            "id": "gt0015",
            "text": "NIH Stroke Scale (NIHSS) ≥ 10",
            "description": "NIHSS: NIH Stroke Scale."
          },
          "gt0016": {
            "id": "gt0016",
            "text": "SEDAN Score",
            "description": "*"
          },
          "gt0017": {
            "id": "gt0017",
            "text": "Set 0 to Blood sugar  8 mmol/l (144 mg/dl) "
          },
          "gt0018": {
            "id": "gt0018",
            "text": "Set 1 to Blood sugar 8.1–12mmol/l (145–216 mg/dl)"
          },
          "gt0019": {
            "id": "gt0019",
            "text": "Set 2 to Blood sugar  >12 mmol/l (>216 mg/dl)"
          },
          "gt0020": {
            "id": "gt0020",
            "text": "Set 0 to Age<=75 years "
          },
          "gt0021": {
            "id": "gt0021",
            "text": "Set 1 to Age>75 years "
          },
          "gt0022": {
            "id": "gt0022",
            "text": "Set 0 to NIHSS on admission 0-9"
          },
          "gt0023": {
            "id": "gt0023",
            "text": "Set 1 to NIHSS on admission>=10"
          },
          "gt0024": {
            "id": "gt0024",
            "text": "Set SEDAN score calculation"
          }
        }
      }
    }
  }
}