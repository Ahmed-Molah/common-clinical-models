        ],
        "then": [
          "$gt0012|2x HCT score|=10|local::at0049|<32% depending on age and sex|"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 7,
        "when": [
          "$gt0029|Erythrocyte sedimentation rate (ESR)|<20,mm/h"
        ],
        "then": [
          "$gt0013|ESR score|=0|local::at0050|< 20|"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 6,
        "when": [
          "$gt0029|Erythrocyte sedimentation rate (ESR)|>=20,mm/h",
          "$gt0029|Erythrocyte sedimentation rate (ESR)|<=50,mm/h"
        ],
        "then": [
          "$gt0013|ESR score|=5|local::at0051|20-50|"
        ]
      },
      "gt0048": {
        "id": "gt0048",
        "priority": 5,
        "when": [
