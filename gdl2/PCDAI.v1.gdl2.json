        ],
        "then": [
          "$gt0014|Albumin score|=10|local::at0055|≤3.0 (30)|"
        ]
      },
      "gt0052": {
        "id": "gt0052",
        "priority": 1,
        "then": [
          "$gt0015|Total Score|.precision=2",
