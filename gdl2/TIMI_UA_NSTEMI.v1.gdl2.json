    "rules": {
      "gt0023": {
        "id": "gt0023",
        "priority": 11,
        "when": [
          "$gt0014|Known CAD (Stenosis ≥ 50%)|==null",
          "$gt0021|Positive Cardiac Marker|==null",
          "$gt0011|≥ 3 CAD Risk Factors*|==null",
          "$gt0020|EKG ST changes ≥ 0.5mm|==null",
          "$gt0018|ASA Use in Past 7 days|==null",
          "$gt0019|Severe angina (≥ 2 episodes in 24 hrs)|==null",
          "$gt0009|Age ≥ 65|==null"
        ],
        "then": [
          "$gt0021|Positive Cardiac Marker|=0|local::at0020|No|",
          "$gt0011|≥ 3 CAD Risk Factors*|=0|local::at0010|No|",
          "$gt0020|EKG ST changes ≥ 0.5mm|=0|local::at0018|No|",
          "$gt0018|ASA Use in Past 7 days|=0|local::at0014|No|",
          "$gt0019|Severe angina (≥ 2 episodes in 24 hrs)|=0|local::at0016|No|",
          "$gt0009|Age ≥ 65|=0|local::at0024|No|",
          "$gt0014|Known CAD (Stenosis ≥ 50%)|=0|local::at0012|No|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 10,
        "when": [
          "$gt0005|Birthdate|!=null"
        ],
        "then": [
          "$gt0004|Age|.magnitude=$currentDateTime.year-$gt0005.year",
          "$gt0004|Age|.unit='a'"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 9,
        "when": [
          "$gt0004|Age|>=65,a"
        ],
        "then": [
          "$gt0009|Age ≥ 65|=1|local::at0025|Yes|"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 8,
        "when": [
          "$gt0004|Age|<65,a"
        ],
        "then": [
          "$gt0009|Age ≥ 65|=0|local::at0024|No|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 7,
        "when": [
          "$gt0008|≥ 3 CAD Risk Factors*|!=null"
        ],
        "then": [
          "$gt0011|≥ 3 CAD Risk Factors*|=$gt0008|≥ 3 CAD Risk Factors*|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 6,
        "when": [
          "$gt0010|Known CAD (Stenosis ≥ 50%)|!=null"
        ],
        "then": [
          "$gt0014|Known CAD (Stenosis ≥ 50%)|=$gt0010|Known CAD (Stenosis ≥ 50%)|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 5,
        "when": [
          "$gt0012|ASA Use in Past 7 days|!=null"
        ],
        "then": [
          "$gt0018|ASA Use in Past 7 days|=$gt0012|ASA Use in Past 7 days|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 4,
        "when": [
          "$gt0015|Severe angina (≥ 2 episodes in 24 hrs)|!=null"
        ],
        "then": [
          "$gt0019|Severe angina (≥ 2 episodes in 24 hrs)|=$gt0015|Severe angina (≥ 2 episodes in 24 hrs)|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 3,
        "when": [
          "$gt0016|EKG ST changes ≥ 0.5mm|!=null"
        ],
        "then": [
          "$gt0020|EKG ST changes ≥ 0.5mm|=$gt0016|EKG ST changes ≥ 0.5mm|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 2,
        "when": [
          "$gt0017|Positive Cardiac Marker|!=null"
        ],
        "then": [
          "$gt0021|Positive Cardiac Marker|=$gt0017|Positive Cardiac Marker|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 1,
        "then": [
          "$gt0022|Total score|.magnitude=((((($gt0021.value+$gt0011.value)+$gt0020.value)+$gt0018.value)+$gt0019.value)+$gt0009.value)+$gt0014.value"
        ]
      }
    }
