    "rules": {
      "gt0028": {
        "id": "gt0028",
        "priority": 7,
        "when": [
          "$gt0003|Weight|!=null",
          "$gt0005|Height/Length|!=null",
          "$gt0003|Weight|.unit=='kg'",
          "$gt0005|Height/Length|.unit=='cm'"
        ],
        "then": [
          "$gt0020|BSA Score|.magnitude=($gt0005.magnitude*$gt0003.magnitude/3600)^0.5",
          "$gt0020|BSA Score|.precision=2",
          "$gt0020|BSA Score|.unit='m2'"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 6,
        "when": [
          "$gt0017|Age|<70,a",
          "$gt0017|Age|.unit=='a'"
        ],
        "then": [
          "$gt0022|VO2|.magnitude=125*$gt0020.magnitude",
          "$gt0022|VO2|.unit='ml/min/m2'"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 5,
        "when": [
          "$gt0017|Age|>=70,a",
          "$gt0017|Age|.unit=='a'"
        ],
        "then": [
          "$gt0022|VO2|.magnitude=$gt0020.magnitude*110",
          "$gt0022|VO2|.unit='ml/min/m2'"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 4,
        "when": [
          "$gt0009|SaO2|!=null",
          "$gt0011|SvO₂|!=null",
          "$gt0013|Haemoglobin|!=null",
          "$gt0013|Haemoglobin|.unit=='gm/dl'"
        ],
        "then": [
          "$gt0030|A-V O2 content difference|.magnitude=($gt0009.magnitude/100-$gt0011.magnitude/100)*$gt0013.magnitude*13.4",
          "$gt0030|A-V O2 content difference|.precision=2"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 3,
        "when": [
          "fired($gt0031)"
        ],
        "then": [
          "$gt0023|Cardiac Output|.magnitude=$gt0022.magnitude/$gt0030.magnitude",
          "$gt0023|Cardiac Output|.precision=2",
          "$gt0023|Cardiac Output|.unit='l/min'"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 2,
        "when": [
          "fired($gt0032)"
        ],
        "then": [
          "$gt0024|Cardiac Index|.magnitude=$gt0023.magnitude/$gt0020.magnitude",
          "$gt0024|Cardiac Index|.precision=1",
          "$gt0024|Cardiac Index|.unit='l/min/m2'"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 1,
        "when": [
          "fired($gt0032)",
          "$gt0015|Heart rate|!=null"
        ],
        "then": [
          "$gt0025|Stroke Volume|.magnitude=($gt0023.magnitude*1000)/$gt0015.magnitude",
          "$gt0025|Stroke Volume|.unit='ml/beat'"
        ]
      }
    }
  },
  "ontology": {
    "term_definitions": {
      "en": {
        "id": "en",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "Cardiac Output (Fick’s Formula)",
            "description": "Fick's formula for determining cardiac output, cardiac index, and stroke volume is: \nCardiac Output (CO), L/min=  O2 consumption/ (A-V) O2 content difference\nwhere: \nin patients <70 years, VO2   mL/min/m2= 125 mL O2/min * BSA, \nin patients >=70 years, VO2  mL/min/m2= 110 mL O2/min * BSA\nBody Surface Area (BSA), m2= [(Height, cm x Weight, kg)/ 3,600 ]^0.5\n(A-V) O2 content difference= (SaO2-SvO2)*Hb*13.4\n\nCI (Cardiac Index), L/min/m2 = CO/BSA\nSV (Stroke Volume), mL/beat = CO/HR\n\nSaO2 should be acquired by arterial blood gas measurement. SvO2 should be acquired by mixed venous gas from pulmonary artery catheter. The formula assumes decimal values, not percentages."
          },
          "gt0003": {
            "id": "gt0003",
            "text": "Weight",
            "description": "The weight of the individual."
