          },
          "gt0023": {
            "id": "gt0023",
            "text": "Set HR<=90"
          },
          "gt0024": {
            "id": "gt0024",
            "text": "Set ALT70 Score"
          },
          "gt0025": {
            "id": "gt0025",
            "text": "Set interpretation score 0-2"
          },
          "gt0026": {
