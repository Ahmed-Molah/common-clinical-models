    "rules": {
      "gt0018": {
        "id": "gt0018",
        "priority": 10,
        "when": [
          "$gt0006|Age|>=70,a"
        ],
        "then": [
          "$gt0012|Age ≥70 years|=2|local::at0009|Yes|"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 9,
        "when": [
          "$gt0006|Age|<70,a"
        ],
        "then": [
          "$gt0012|Age ≥70 years|=0|local::at0008|No|"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 8,
        "when": [
          "$gt0008|White cell count|>=10,10*9/l"
        ],
        "then": [
          "$gt0013|WBC in ED ≥10,000/µL|=1|local::at0012|Yes|"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 7,
        "when": [
          "$gt0008|White cell count|<10,10*9/l"
        ],
        "then": [
          "$gt0013|WBC in ED ≥10,000/µL|=0|local::at0011|No|"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 6,
        "when": [
          "$gt0010|Heart rate|>=90,/min"
        ],
        "then": [
          "$gt0014|HR in ED ≥90 bpm|=1|local::at0015|Yes|"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 5,
        "when": [
          "$gt0010|Heart rate|<90,/min"
        ],
        "then": [
          "$gt0014|HR in ED ≥90 bpm|=0|local::at0014|No|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 4,
        "when": [
          "$gt0004|Asymmetric|!=null",
          "$gt0006|Age|!=null",
          "$gt0008|White cell count|!=null",
          "$gt0010|Heart rate|!=null"
        ],
        "then": [
          "$gt0015|ALT-70 Score|.magnitude=$gt0004.value+$gt0012.value+$gt0013.value+$gt0014.value"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 3,
        "when": [
          "$gt0015|ALT-70 Score|<3"
        ],
        "then": [
          "$gt0017|Recommendations|=0|local::at0003|Reassess (>83.3% likelihood of pseudocellulitis)|"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 2,
        "when": [
          "$gt0015|ALT-70 Score|>=3",
          "$gt0015|ALT-70 Score|<5"
        ],
        "then": [
          "$gt0017|Recommendations|=1|local::at0004|Consult dermatology and/or infectious disease|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 1,
        "when": [
          "$gt0015|ALT-70 Score|>=5",
          "$gt0015|ALT-70 Score|<=7"
        ],
        "then": [
          "$gt0017|Recommendations|=2|local::at0005|Treat (>82.2% likelihood of true cellulitis)|"
        ]
      }
    }
  },
  "ontology": {
    "term_definitions": {
      "en": {
        "id": "en",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "Alt 70 score for cellulitis",
            "description": "Developed to assist with evaluation of lower extremity redness, which may be inappropriately diagnosed as cellulitis (versus mimickers, or “pseudocellulitis”).  \nMost patients with cellulitis have acute onset, unilateral involvement (usually one leg), and are sick with an elevated white blood cell count, tachycardia, and/or a fever.\nPredicts likelihood of lower extremity cellulitis over other diagnoses.\nAdult patients presenting to the ED with a red leg and clinical concern for cellulitis.\nVariable and points: \nAsymmetric: no=0, yes= 3\nAge ≥70 years: no=0, yes= 2\nWBC in ED ≥10,000/µL: no=0, yes= 1\nHR in ED ≥90 bpm: no=0, yes= 1\n\nAlt 70 score for cellulitis=Addition of the selected points\n\nInterpretation:\nALT-70 Scores and related recommendations:\n0-2=Reassess (>83.3% likelihood of pseudocellulitis)\n3-4=Consult dermatology and/or infectious disease\n5-7=Treat (>82.2% likelihood of true cellulitis)"
          },
          "gt0004": {
            "id": "gt0004",
            "text": "Asymmetric",
            "description": "Not symmetric; denoting a lack of symmetry between two or more like parts."
          },
          "gt0006": {
            "id": "gt0006",
            "text": "Age",
            "description": "Age in years, and for babies: months, weeks or days"
          },
          "gt0008": {
            "id": "gt0008",
            "text": "White cell count",
            "description": "The number of white cells per litre"
          },
          "gt0010": {
            "id": "gt0010",
            "text": "Heart rate",
            "description": "The rate, measured in beats per minute."
          },
          "gt0012": {
            "id": "gt0012",
            "text": "Age ≥70 years",
            "description": "*"
          },
          "gt0013": {
            "id": "gt0013",
            "text": "WBC in ED ≥10,000/µL",
            "description": "*"
          },
          "gt0014": {
            "id": "gt0014",
            "text": "HR in ED ≥90 bpm",
            "description": "*"
          },
          "gt0015": {
            "id": "gt0015",
            "text": "ALT-70 Score",
            "description": "*"
          },
          "gt0017": {
            "id": "gt0017",
            "text": "Recommendations",
            "description": "*"
          },
          "gt0018": {
            "id": "gt0018",
            "text": "Set Age >=70"
          },
          "gt0019": {
            "id": "gt0019",
            "text": "Set Age <=70"
          },
          "gt0020": {
            "id": "gt0020",
            "text": "Set WBC>=10000"
          },
