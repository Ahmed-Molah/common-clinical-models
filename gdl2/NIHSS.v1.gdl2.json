  "ontology": {
    "term_definitions": {
      "sv": {
        "id": "sv",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "National Institutes of Health Stroke Scale",
            "description": "National Institutes of Health Stroke Scale är ett instrument som används för att skatta och utvärdera svårighetsgrad av neurologisk påverkan vid stroke. NIHSS består av femton parametrar erhållna via klinisk neurologisk undersökning."
          },
          "gt0004": {
            "id": "gt0004",
            "text": "1A - Vakenhetsgrad",
            "description": "*(en) May be assessed during the course of the examination."
          },
          "gt0005": {
            "id": "gt0005",
            "text": "1B - Orientering",
            "description": "*(en) Ask the patient for month and age."
          },
          "gt0006": {
            "id": "gt0006",
            "text": "1C - Förståelse",
            "description": "*(en) Ask the patient to blink eyes and squeeze hands."
          },
          "gt0007": {
            "id": "gt0007",
            "text": "2 - Ögonmotorik",
            "description": "*(en) Test the patients' horizontal eye movement."
          },
          "gt0008": {
            "id": "gt0008",
            "text": "3 - Synfält",
            "description": "*(en) Test visual field of both eyes."
          },
          "gt0009": {
            "id": "gt0009",
            "text": "4 - Facialispares",
            "description": "*(en) Assess symmetry of facial expressions."
          },
          "gt0010": {
            "id": "gt0010",
            "text": "5A - Pares vänster arm",
            "description": "*(en) Ask the patient to extend the arm with palm facing downwards, and hold it still."
          },
          "gt0011": {
            "id": "gt0011",
            "text": "5B - Pares höger arm",
            "description": "*(en) Ask the patient to extend the arm with palm facing downwards, and hold it still."
          },
          "gt0012": {
            "id": "gt0012",
            "text": "6A - Pares vänster ben",
            "description": "*(en) Ask the patient to place the leg 30 degrees above horizontal, and hold it still."
          },
          "gt0013": {
            "id": "gt0013",
            "text": "6B - Pares höger ben",
            "description": "*(en) Ask the patient to place the leg 30 degrees above horizontal, and hold it still."
          },
          "gt0014": {
            "id": "gt0014",
            "text": "7 - Extremitetsataxi",
            "description": "*(en) Tests: finger-nose and heel-shin."
          },
          "gt0015": {
            "id": "gt0015",
            "text": "8 - Sensibilitet",
            "description": "*(en) Test on all four limbs with pinpricks."
          },
          "gt0016": {
            "id": "gt0016",
            "text": "9 - Språk",
            "description": "*(en) Use the picture, sentences and words provided in the NIHSS manual."
          },
          "gt0017": {
            "id": "gt0017",
            "text": "10 - Dysartri",
            "description": "*(en) Ask the patient to read the words provided in the NIHSS manual."
          },
          "gt0018": {
            "id": "gt0018",
            "text": "11 - Sensoriskt neglekt",
            "description": "*(en) Use the content in the NIHSS manual."
          },
