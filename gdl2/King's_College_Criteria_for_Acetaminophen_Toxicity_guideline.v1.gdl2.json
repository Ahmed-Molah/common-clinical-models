    "rules": {
      "gt0022": {
        "id": "gt0022",
        "priority": 6,
        "when": [
          "$gt0003|Arterial pH|<7.3,[pH]"
        ],
        "then": [
          "$gt0016|Arterial pH < 7.30|=1|local::at0006|Yes|"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 5,
        "when": [
          "($gt0005|INR|>'6.5')||($gt0006|PT|>100,s)"
        ],
        "then": [
          "$gt0017|INR > 6.5 (PT > 100 sec)|=1|local::at0009|Yes|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 4,
        "when": [
          "($gt0008|Creatinine|.magnitude>300)||($gt0008|Creatinine|.magnitude>3.4)"
        ],
        "then": [
          "$gt0018|Creatinine 3.4 mg/dL (300 µmol/L)|=1|local::at0012|Yes|"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 3,
        "when": [
          "($gt0010|Grades|==3|local::at0005|Grade III|)||($gt0010|Grades|==4|local::at0006|Grade IV|)"
        ],
        "then": [
          "$gt0019|Grade III or IV hepatic encephalopathy|=1|local::at0015|Yes|"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 2,
        "when": [
          "$gt0003|Arterial pH|!=null",
          "($gt0005|INR|!=null)||($gt0006|PT|!=null)",
          "$gt0008|Creatinine|!=null",
          "$gt0010|Grades|!=null"
        ],
        "then": [
          "$gt0020|Criteria|.magnitude=$gt0016.value+$gt0017.value+$gt0018.value+$gt0019.value"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 1
      }
    }
