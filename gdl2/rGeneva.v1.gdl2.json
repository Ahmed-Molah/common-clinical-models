    "rules": {
      "gt0025": {
        "id": "gt0025",
        "priority": 14,
        "when": [
          "$gt0017|Pain on limb palpation|==null",
          "$gt0016|Heart rate variable|==null",
          "$gt0010|Age > 65 years?|==null",
          "$gt0011|Previous DVT or PE|==null",
          "$gt0012|Surgery (under General)|==null",
          "$gt0013|Active malignant condition|==null",
          "$gt0014|Unilateral lower limb pain|==null",
          "$gt0015|Hemoptysis|==null"
        ],
        "then": [
          "$gt0010|Age > 65 years?|=0|local::at0014|No|",
          "$gt0011|Previous DVT or PE|=0|local::at0016|No|",
          "$gt0012|Surgery (under General)|=0|local::at0018|No|",
          "$gt0013|Active malignant condition|=0|local::at0020|No|",
          "$gt0014|Unilateral lower limb pain|=0|local::at0022|No|",
          "$gt0015|Hemoptysis|=0|local::at0024|No|",
          "$gt0016|Heart rate variable|=0|local::at0026|<75|",
          "$gt0017|Pain on limb palpation|=0|local::at0029|No|"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 13,
        "when": [
          "$gt0020|Birthdate|!=null"
        ],
        "then": [
          "$gt0022|Age|.magnitude=$currentDateTime.year-$gt0020.year",
          "$gt0022|Age|.unit='a'"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 12,
        "when": [
          "$gt0022|Age|<=65,a",
          "$gt0022|Age|!=null"
        ],
        "then": [
          "$gt0010|Age > 65 years?|=0|local::at0014|No|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 11,
        "when": [
          "$gt0022|Age|>65,a",
          "$gt0022|Age|!=null"
        ],
        "then": [
          "$gt0010|Age > 65 years?|=1|local::at0015|Yes|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 10,
        "when": [
          "$gt0024|Heart Rate|<75,/min",
          "$gt0024|Heart Rate|!=null"
        ],
        "then": [
          "$gt0016|Heart rate variable|=0|local::at0026|<75|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 9,
        "when": [
          "$gt0024|Heart Rate|<=94,/min",
          "$gt0024|Heart Rate|>=75,/min",
          "$gt0024|Heart Rate|!=null"
        ],
        "then": [
          "$gt0016|Heart rate variable|=3|local::at0027|75-94|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 8,
        "when": [
          "$gt0024|Heart Rate|>=95,/min",
          "$gt0024|Heart Rate|!=null"
        ],
        "then": [
          "$gt0016|Heart rate variable|=5|local::at0028|≥ 95|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 7,
        "when": [
          "$gt0003|Previous DVT or PE?|!=null"
        ],
        "then": [
          "$gt0011|Previous DVT or PE|=$gt0003|Previous DVT or PE?|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 6,
        "when": [
          "$gt0004|Surgery (under General)?|!=null"
        ],
        "then": [
          "$gt0012|Surgery (under General)|=$gt0004|Surgery (under General)?|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 5,
        "when": [
          "$gt0005|Active malignant condition?|!=null"
        ],
        "then": [
          "$gt0013|Active malignant condition|=$gt0005|Active malignant condition?|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 4,
        "when": [
          "$gt0006|Unilateral lower limb pain?|!=null"
        ],
        "then": [
          "$gt0014|Unilateral lower limb pain|=$gt0006|Unilateral lower limb pain?|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 3,
        "when": [
          "$gt0007|Hemoptysis?|!=null"
        ],
        "then": [
          "$gt0015|Hemoptysis|=$gt0007|Hemoptysis?|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 2,
        "when": [
          "$gt0008|Pain on limb palpation?|!=null"
        ],
        "then": [
          "$gt0017|Pain on limb palpation|=$gt0008|Pain on limb palpation?|"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 1,
        "then": [
          "$gt0018|Total score|.magnitude=(((((($gt0010.value+$gt0011.value)+$gt0012.value)+$gt0013.value)+$gt0014.value)+$gt0015.value)+$gt0016.value)+$gt0017.value"
        ]
      }
    }
