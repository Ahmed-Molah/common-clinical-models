    "rules": {
      "gt0083": {
        "id": "gt0083",
        "priority": 21,
        "when": [
          "$gt0061|Age 16-45|==null",
          "$gt0062|History of preadolescent sexual abuse|==null",
          "$gt0063|History of depression|==null",
          "$gt0064|History of ADD,OCD,Bipolar or Schizophrenia|==null",
          "$gt0065|History of alcohol abuse|==null",
          "$gt0066|History of illegal drug abuse|==null",
          "$gt0084|History of prescription drug use|==null",
          "$gt0067|Family history alcohol abuse|==null",
          "$gt0086|Family history illegal drug abuse|==null",
          "$gt0069|Family history prescription drug abuse|==null"
        ],
        "then": [
          "$gt0061|Age 16-45|=0|local::at0017|No|",
          "$gt0062|History of preadolescent sexual abuse|=0|local::at0019|No|",
          "$gt0063|History of depression|=0|local::at0021|No|",
          "$gt0064|History of ADD,OCD,Bipolar or Schizophrenia|=0|local::at0023|No|",
          "$gt0065|History of alcohol abuse|=0|local::at0025|No|",
          "$gt0066|History of illegal drug abuse|=0|local::at0027|No|",
          "$gt0084|History of prescription drug use|=0|local::at0029|No|",
          "$gt0069|Family history prescription drug abuse|=0|local::at0037|No|",
          "$gt0067|Family history alcohol abuse|=0|local::at0039|No|",
          "$gt0086|Family history illegal drug abuse|=0|local::at0041|No|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 20,
        "when": [
          "$gt0027|Birthdate|!=null"
        ],
        "then": [
          "$gt0035|Age|.magnitude=$currentDateTime.year-$gt0027.year",
          "$gt0035|Age|.unit='a'"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 19,
        "when": [
          "$gt0035|Age|.magnitude>=16",
          "$gt0035|Age|.magnitude<=45",
          "$gt0035|Age|!=null"
        ],
        "then": [
          "$gt0061|Age 16-45|=1|local::at0018|Yes|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 18,
        "when": [
          "($gt0035|Age|.magnitude<16)||($gt0035|Age|.magnitude>45)",
          "$gt0035|Age|!=null"
        ],
        "then": [
          "$gt0061|Age 16-45|=0|local::at0017|No|"
        ]
      },
      "gt0039": {
