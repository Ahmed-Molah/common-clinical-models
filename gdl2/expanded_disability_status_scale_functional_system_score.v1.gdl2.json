    "rules": {
      "gt0015": {
        "id": "gt0015",
        "priority": 34,
        "when": [
          "$gt0004|Ambulation|==local::at0005|0-Fully ambulatory|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=0,1"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 33,
        "when": [
          "$gt0004|Ambulation|==local::at0006|4.0-Fully ambulatory, self-sufficient, up 12 hours a day despite relatively severe disability. Able to walk ≥500 meters without aid/rest.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=4,1"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 32,
        "when": [
          "$gt0004|Ambulation|==local::at0007|4.5-Fully ambulatory, able to work a full day, may require minimal assistance. Able to walk ≥300 meters without aid/rest.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=4.5,1"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 31,
        "when": [
          "$gt0004|Ambulation|==local::at0008|5.0-Ambulatory for 200 meters without aid/rest; disability that impairs full daily activities (eg, to work full day without special provisions).|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=5,1"
        ]
      },
      "gt0018": {
        "id": "gt0018",
        "priority": 30,
        "when": [
          "$gt0004|Ambulation|==local::at0009|5.5-Ambulatory for 100 meters without aid/rest; disability precludes full daily activities.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=5.5,1",
          "$gt0013|Ambulation Score|.precision=1"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 29,
        "when": [
          "$gt0004|Ambulation|==local::at0010|6.0-Intermittent or unilateral constant assistance required to walk 100 meters, with/without resting.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=6,1"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 28,
        "when": [
          "$gt0004|Ambulation|==local::at0011|6.5-Constant bilateral assistance (canes, crutches, or braces) required to walk 20 meters without resting.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=6.5,1"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 27,
        "when": [
          "$gt0004|Ambulation|==local::at0012|7.0-Unable to walk >5 meters even with aid; (restricted to wheelchair); wheels self in standard wheelchair and transfers alone.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=7,1"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 26,
        "when": [
          "$gt0004|Ambulation|==local::at0013|7.5-Unable to take more than steps; restricted to wheelchair; may need aid in transfer; wheels self but cannot use standard wheelchair a full day; may require motorized wheelchair.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=7.5,1"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 25,
        "when": [
          "$gt0004|Ambulation|==local::at0014|8.0-Restricted to bed/chair or perambulated in wheelchair, but may be out of bed itself much of the day; retains many self-care functions; generally has effective use of arms.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=8,1"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 24,
        "when": [
          "$gt0004|Ambulation|==local::at0015|8.5-Restricted to bed much of the day; has some effective use of arms; retains some self-care functions.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=8.5,1"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 23,
        "when": [
          "$gt0004|Ambulation|==local::at0016|9.0-Helpless bed patient; can communicate and eat.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=9,1"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 22,
        "when": [
          "$gt0004|Ambulation|==local::at0017|9.5-Totally helpless bed patient; unable to communicate effectively or eat/swallow.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=9.5,1"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 21,
        "when": [
          "$gt0004|Ambulation|==local::at0018|10.0-Death due to MS.|"
        ],
        "then": [
          "$gt0013|Ambulation Score|=10,1"
        ]
      },
      "gt0045": {
        "id": "gt0045",
        "priority": 20,
        "then": [
          "$gt0034|No.of 0 in input|.magnitude=0.5"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 19
      },
      "gt0040": {
        "id": "gt0040",
        "priority": 18,
        "then": [
          "$gt0035|No.of 2 in input|.magnitude=0.5"
        ]
      },
      "gt0041": {
        "id": "gt0041",
        "priority": 17,
        "then": [
          "$gt0037|No.of 3 in input|.magnitude=0.5"
        ]
      },
      "gt0042": {
        "id": "gt0042",
        "priority": 16,
        "then": [
          "$gt0038|No.of 4 in input|.magnitude=0.5"
        ]
      },
      "gt0043": {
        "id": "gt0043",
        "priority": 15,
        "then": [
          "$gt0032|No.of 5 in input|.magnitude=0.5"
        ]
      },
      "gt0044": {
        "id": "gt0044",
        "priority": 14,
        "then": [
          "$gt0036|No.of 6 in input|.magnitude=0.5"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 13,
        "when": [
          "($gt0034|No.of 0 in input|==8,1)||($gt0034|No.of 0 in input|==7,1)",
          "$gt0003|Cerebral|==1|local::at0068|Mood alteration only (Does not affect DSS score) +1|"
        ],
        "then": [
          "$gt0014|Functional System Score|=0,1"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 12,
        "when": [
          "((($gt0033|No.of 1 in input|==1,1)&&($gt0034|No.of 0 in input|==7,1))&&($gt0003|Cerebral|==0|local::at0067|Normal +0|))||((($gt0033|No.of 1 in input|==2,1)&&($gt0034|No.of 0 in input|==6,1))&&($gt0003|Cerebral|==1|local::at0068|Mood alteration only (Does not affect DSS score) +1|))"
        ],
        "then": [
          "$gt0014|Functional System Score|=1,1"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 11,
        "when": [
          "(($gt0033|No.of 1 in input|>1,1)&&($gt0003|Cerebral|==0|local::at0067|Normal +0|))||(($gt0033|No.of 1 in input|>2,1)&&($gt0003|Cerebral|==1|local::at0068|Mood alteration only (Does not affect DSS score) +1|))",
          "$gt0036|No.of 6 in input|==0,1",
          "$gt0032|No.of 5 in input|==0,1",
          "$gt0038|No.of 4 in input|==0,1",
          "$gt0037|No.of 3 in input|==0,1",
          "$gt0035|No.of 2 in input|==0,1"
        ],
        "then": [
          "$gt0014|Functional System Score|=1.5,1"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 10,
        "when": [
          "$gt0035|No.of 2 in input|==1,1",
          "$gt0036|No.of 6 in input|==0,1",
          "$gt0032|No.of 5 in input|==0,1",
          "$gt0038|No.of 4 in input|==0,1",
          "$gt0037|No.of 3 in input|==0,1",
          "$gt0034|No.of 0 in input|!=null",
          "$gt0033|No.of 1 in input|!=null"
        ],
        "then": [
          "$gt0014|Functional System Score|=2,1"
        ]
      },
      "gt0048": {
        "id": "gt0048",
        "priority": 9,
        "when": [
          "$gt0035|No.of 2 in input|==2,1",
          "$gt0036|No.of 6 in input|==0,1",
          "$gt0032|No.of 5 in input|==0,1",
          "$gt0038|No.of 4 in input|==0,1",
          "$gt0037|No.of 3 in input|==0,1",
          "$gt0034|No.of 0 in input|!=null",
          "$gt0033|No.of 1 in input|!=null"
        ],
        "then": [
          "$gt0014|Functional System Score|=2.5,1"
        ]
      },
      "gt0049": {
        "id": "gt0049",
        "priority": 8,
        "when": [
          "(($gt0037|No.of 3 in input|==1,1)&&($gt0035|No.of 2 in input|==0,1))||((($gt0035|No.of 2 in input|>=3,1)||($gt0035|No.of 2 in input|<=4,1))&&($gt0037|No.of 3 in input|==0,1))",
          "$gt0036|No.of 6 in input|==0,1",
          "$gt0032|No.of 5 in input|==0,1",
          "$gt0038|No.of 4 in input|==0,1",
          "$gt0034|No.of 0 in input|!=null",
          "$gt0033|No.of 1 in input|!=null"
        ],
        "then": [
          "$gt0014|Functional System Score|=3,1"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 7,
        "when": [
          "(((($gt0037|No.of 3 in input|==1,1)&&($gt0035|No.of 2 in input|>=1,1))&&($gt0035|No.of 2 in input|<=2,1))||(($gt0037|No.of 3 in input|==2,1)&&($gt0035|No.of 2 in input|==0,1)))||(($gt0037|No.of 3 in input|==0,1)&&($gt0035|No.of 2 in input|==5,1))",
          "$gt0036|No.of 6 in input|==0,1",
          "$gt0032|No.of 5 in input|==0,1",
          "$gt0038|No.of 4 in input|==0,1",
          "$gt0034|No.of 0 in input|!=null",
          "$gt0033|No.of 1 in input|!=null"
        ],
        "then": [
          "$gt0014|Functional System Score|=3.5,1"
        ]
      },
      "gt0051": {
        "id": "gt0051",
        "priority": 6,
        "when": [
          "$gt0038|No.of 4 in input|==1,1",
          "$gt0036|No.of 6 in input|==0,1",
          "$gt0032|No.of 5 in input|==0,1",
          "$gt0038|No.of 4 in input|==0,1",
          "$gt0037|No.of 3 in input|==0,1",
          "$gt0035|No.of 2 in input|==0,1",
          "$gt0033|No.of 1 in input|!=null",
          "$gt0034|No.of 0 in input|!=null"
        ],
        "then": [
          "$gt0064|Functional System Score Interval|='4 to 4.5'"
        ]
      },
      "gt0053": {
        "id": "gt0053",
        "priority": 5,
        "when": [
          "$gt0032|No.of 5 in input|==1,1",
          "$gt0036|No.of 6 in input|==0,1",
          "$gt0038|No.of 4 in input|==0,1",
          "$gt0037|No.of 3 in input|==0,1",
          "$gt0035|No.of 2 in input|==0,1",
          "$gt0033|No.of 1 in input|!=null",
          "$gt0034|No.of 0 in input|!=null"
        ],
        "then": [
          "$gt0064|Functional System Score Interval|='5 to 5.5'"
        ]
      },
      "gt0055": {
        "id": "gt0055",
        "priority": 4,
        "when": [
          "$gt0037|No.of 3 in input|>2,1",
          "$gt0036|No.of 6 in input|<2,1",
          "$gt0032|No.of 5 in input|<2,1",
          "$gt0038|No.of 4 in input|<2,1",
          "$gt0033|No.of 1 in input|!=null",
          "$gt0034|No.of 0 in input|!=null",
          "$gt0035|No.of 2 in input|!=null"
        ],
        "then": [
          "$gt0064|Functional System Score Interval|='6 to 6.5'"
        ]
      },
      "gt0057": {
        "id": "gt0057",
        "priority": 3,
        "when": [
          "((((($gt0038|No.of 4 in input|==2,1)||(($gt0038|No.of 4 in input|==1,1)&&($gt0032|No.of 5 in input|==1,1)))||($gt0032|No.of 5 in input|==2,1))||(($gt0032|No.of 5 in input|==1,1)&&($gt0036|No.of 6 in input|==1,1)))||(($gt0036|No.of 6 in input|==1,1)&&($gt0038|No.of 4 in input|==1,1)))||($gt0036|No.of 6 in input|==2,1)"
        ]
      },
      "gt0060": {
        "id": "gt0060",
        "priority": 2,
        "when": [
          "$gt0038|No.of 4 in input|==3,1",
          "$gt0038|No.of 4 in input|==2,1",
          "$gt0038|No.of 4 in input|==1,1",
          "$gt0032|No.of 5 in input|==3,1",
          "$gt0032|No.of 5 in input|==2,1",
          "$gt0032|No.of 5 in input|==1,1",
          "$gt0036|No.of 6 in input|==3,1",
          "$gt0036|No.of 6 in input|==2,1",
          "$gt0036|No.of 6 in input|==1,1"
        ]
      },
      "gt0062": {
        "id": "gt0062",
        "priority": 1,
        "when": [
          "$gt0038|No.of 4 in input|==4,1",
          "$gt0032|No.of 5 in input|==4,1",
          "$gt0036|No.of 6 in input|==4,1"
        ],
        "then": [
          "$gt0064|Functional System Score Interval|='9 to 10'"
        ]
      }
    }
  },
