archetype (adl_version=1.4)
	openEHR-EHR-EVALUATION.save_score.v0

concept
	[at0000]	-- Save score
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Rashmi Damodaran">
		["email"] = <"rashmi.damodaran@cambio.se">
		["organisation"] = <"Cambio CDS">
		["date"] = <"2020-10-29">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To stratify patients to Risk classes (I-V) and predict in-hospital survival after VA ECMO for refractory cardiogenic shock based on the calculated SAVE score.">
			use = <"To make assessment based on SAVE score.

Interpretations: 

SAVE Score        Risk class        In-hospital survival  	
      
    >5                          I                         75%
  1 to 5                      II                         58%
  -4 to 0                    III                         42%
  -9 to -5                   IV                         30%
    ≤-10                      V                         18%
 
">
			keywords = <"SAVE Score", "ECMO", "VA ECMO", "cardiogenic shock", "in-hospital survival", "shock">
			misuse = <"Do not use in patients receiving veno-venous (VV) ECMO">
			copyright = <"@ Cambio CDS">
		>
	>
	lifecycle_state = <"Initial">
	other_contributors = <>
	other_details = <
		["references"] = <"1. Schmidt M, Burrell A, Roberts L, Bailey M, Sheldrake J, Rycus PT, et al. Predicting survival after ECMO for refractory cardiogenic shock: the survival after veno-arterial-ECMO (SAVE)-score. Eur Heart J. 2015 Sep 1;36(33):2246–56.
2. Chen W-C, Huang K-Y, Yao C-W, Wu C-F, Liang S-J, Li C-H, et al. The modified SAVE score: predicting survival using urgent veno-arterial extracorporeal membrane oxygenation within 24 hours of arrival at the emergency department. Crit Care. 2016 Oct 22;20(1):336.">
		["MD5-CAM-1.0.1"] = <"60E342CE1437B8423660AD9A52C954BE">
		["licence"] = <"Cambio Healthcare Systems">
	>

definition
	EVALUATION[at0000] matches {	-- Save score
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- SAVE Score
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::380]>
								list = <
									["1"] = <
										units = <"1">
									>
								>
							>
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Risk class
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008, 	-- I
									at0009, 	-- II
									at0010, 	-- III
									at0011, 	-- IV
									at0012]	-- V
								}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- In-hospital survival
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0014, 	-- 75%
									at0015, 	-- 58%
									at0016, 	-- 42%
									at0017, 	-- 30%
									at0018]	-- 18%
								}
							}
						}
					}
				}
			}
		}
	}

