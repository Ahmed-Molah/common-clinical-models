concept
	[at0000]	-- Recognition of stroke in the emergency room rosier scale
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Therese Lindberg">
		["email"] = <"therese.lindberg@cambio.se">
