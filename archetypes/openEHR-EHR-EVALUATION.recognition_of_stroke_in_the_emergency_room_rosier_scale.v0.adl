definition
	EVALUATION[at0000] matches {	-- Recognition of stroke in the emergency room rosier scale
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Loss of consciousness or syncope
						value matches {
							0|[local::at0003], 	-- No
							1|[local::at0004]  	-- Yes
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Seizure activity
						value matches {
							0|[local::at0006], 	-- No
							1|[local::at0007]  	-- Yes
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- Asymmetric facial weakness
						value matches {
							0|[local::at0009], 	-- No
							1|[local::at0010]  	-- Yes
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Asymmetric arm weakness
						value matches {
							0|[local::at0012], 	-- No
							1|[local::at0013]  	-- Yes
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- Asymmetric leg weakness
						value matches {
							0|[local::at0015], 	-- No
							1|[local::at0016]  	-- Yes
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {	-- Speech disturbance
						value matches {
							0|[local::at0018], 	-- No
							1|[local::at0019]  	-- Yes
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- Visual field defect
						value matches {
							0|[local::at0021], 	-- No
							1|[local::at0022]  	-- Yes
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {	-- ROSIER score
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {	-- Likelihood of stroke
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0025, 	-- Stroke unlikely
									at0026]	-- Stroke possible
								}
							}
						}
					}
				}
			}
		}
	}

