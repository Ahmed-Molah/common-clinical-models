	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the ROSIER Score in order to distinguish between acute stroke and stroke mimics.">
			use = <"Use to record  ROSIER Score. The score is based on 7 parameters: Loss of consciousness or syncope, Seizure activity, Asymmetric facial weakness, Asymmetric arm weakness, Asymmetric leg weakness, Speech disturbance and Visual field defect.

Likelihood of stroke:
ROSIER Score         Interpretation
â‰¤0                         Stroke unlikely
>0                         Stroke possible">
			keywords = <"stroke", "ROSIER", "recognition of stroke in the emergency room ", "scale", "seizure", "weakness", "speech disturbance", "defect">
			misuse = <"">
			copyright = <"@CambioCDS">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
	other_details = <
		["references"] = <"1. Nor AM, Davis J, Sen B, et al. The Recognition of Stroke in the Emergency Room (ROSIER) scale: development and validation of a stroke recognition instrument. Lancet Neurol. 2005;4(11):727-34.">
		["current_contact"] = <"Therese Lindberg, Cambio CDS<therese.lindberg@cambio.se>">
