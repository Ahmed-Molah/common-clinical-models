definition
	EVALUATION[at0000] matches {	-- qSOFA bedömning
		data matches {
			ITEM_TREE[at0001] matches {	-- *Tree(en)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Riskbedömning
						value matches {
							0|[local::at0004], 	-- Inte hög risk
							1|[local::at0005]  	-- Hög risk
