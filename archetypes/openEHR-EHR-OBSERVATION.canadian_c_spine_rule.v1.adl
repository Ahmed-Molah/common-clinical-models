				["at0000"] = <
					text = <"Canadian C-spine Rule (CCR)">
					description = <"The Canadian C-spine Rule (CCR) helps to safely rule out CSI (Cervical spine injury) in alert and stable trauma patients without a need for radiographic imaging.">
				>
				["at0001"] = <
