definition
	EVALUATION[at0000] matches {	-- Patient Safety Screener 3
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Suicide Risk 
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0006, 	-- Not positive for suicidality by ED-SAFE PSS-3
									at0007]	-- Positive for suicidality by ED-SAFE PSS-3
								}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Interpretation Q1
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[local::at0008]}		-- Depressed mood
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Interpretation Q2
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[local::at0012]}		-- Active suicidal ideation
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Interpretation Q3
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0010, 	-- Recent suicide attempt
									at0011]	-- Lifetime suicide attempt
								}
							}
						}
					}
				}
			}
		}
	}

