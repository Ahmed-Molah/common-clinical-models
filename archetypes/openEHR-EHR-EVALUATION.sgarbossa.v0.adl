definition
	EVALUATION[at0000] matches {	-- Sgarbossa's criteria
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Concordant ST elevation > 1mm in leads with a positive QRS complex
						value matches {
							0|[local::at0003], 	-- No
							5|[local::at0004]  	-- Yes
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Concordant ST depression > 1 mm in V1-V3
						value matches {
							0|[local::at0006], 	-- No
							3|[local::at0007]  	-- Yes
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- Excessively discordant ST elevation (or depression) in leads with a negative QRS
						value matches {
							0|[local::at0009], 	-- No
							2|[local::at0010]  	-- Yes
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Sgarbossa's score
						value matches {
							DV_COUNT matches {
								magnitude matches {|0..10|}
							}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

