archetype (adl_version=1.4)
	openEHR-EHR-EVALUATION.expanded_disability_status_scale_functional_systems_score.v0

concept
	[at0000]	-- Expanded disability status scale functional systems score
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Therese Lindberg">
		["email"] = <"therese.lindberg@cambio.se">
		["organisation"] = <"Cambio CDS">
		["date"] = <"2020-07-23">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To describe severity of disability in patients with MS.">
			use = <"Formula:
EDSS: Points assigned based on level of disability.
FSS: Addition of selected points within body system.


Score of 1.0 to 4.5 -- Normal ambulation.
Score of 5.0 to 9.5 -- Impaired ambulation.">
			keywords = <"disability", "status", "scale", "EDSS", "FSS">
			misuse = <"">
			copyright = <"@CambioCDS">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
	other_details = <
		["references"] = <"1. Kurtzke JF. Rating neurologic impairment in multiple sclerosis: an expanded disability status scale (EDSS). Neurology. 1983 Nov;33(11):1444-52.">
		["current_contact"] = <"Therese Lindberg, Cambio CDS<therese.lindberg@cambio.se>">
		["MD5-CAM-1.0.1"] = <"BFAE978C7874A34E50414E6BCD0F9BBB">
	>

definition
	EVALUATION[at0000] matches {	-- Expanded disability status scale functional systems score
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0003] occurrences matches {0..1} matches {	-- Ambulation Scoring
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0002] occurrences matches {0..1} matches {	-- Ambulation
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0005, 	-- 0-Fully ambulatory
											at0006, 	-- 4.0-Fully ambulatory, self-sufficient, up 12 hours a day despite relatively severe disability. Able to walk ≥500 meters without aid/rest.
											at0007, 	-- 4.5-Fully ambulatory, able to work a full day, may require minimal assistance. Able to walk ≥300 meters without aid/rest.
											at0008, 	-- 5.0-Ambulatory for 200 meters without aid/rest; disability that impairs full daily activities (eg, to work full day without special provisions).
											at0009, 	-- 5.5-Ambulatory for 100 meters without aid/rest; disability precludes full daily activities.
											at0010, 	-- 6.0-Intermittent or unilateral constant assistance required to walk 100 meters, with/without resting.
											at0011, 	-- 6.5-Constant bilateral assistance (canes, crutches, or braces) required to walk 20 meters without resting.
											at0012, 	-- 7.0-Unable to walk >5 meters even with aid; (restricted to wheelchair); wheels self in standard wheelchair and transfers alone.
											at0013, 	-- 7.5-Unable to take more than steps; restricted to wheelchair; may need aid in transfer; wheels self but cannot use standard wheelchair a full day; may require motorized wheelchair.
											at0014, 	-- 8.0-Restricted to bed/chair or perambulated in wheelchair, but may be out of bed itself much of the day; retains many self-care functions; generally has effective use of arms.
											at0015, 	-- 8.5-Restricted to bed much of the day; has some effective use of arms; retains some self-care functions.
											at0016, 	-- 9.0-Helpless bed patient; can communicate and eat.
											at0017, 	-- 9.5-Totally helpless bed patient; unable to communicate effectively or eat/swallow.
											at0018]	-- 10.0-Death due to MS.
										}
									}
								}
							}
						}
					}
					CLUSTER[at0004] occurrences matches {0..1} matches {	-- Functional Systems
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0019] occurrences matches {0..1} matches {	-- Pyramidal
								value matches {
									0|[local::at0027], 	-- Normal +0
									1|[local::at0028], 	-- Abnormal signs without disability +1
									2|[local::at0029], 	-- Minimal disability +2
									3|[local::at0030], 	-- Mild or moderate paraparesis or hemiparesis; severe monoparesis +3
									4|[local::at0031], 	-- Marked paraparesis or hemiparesis; moderate quadriparesis; or monoplegia +4
									5|[local::at0032], 	-- Paraplegia, hemiplegia, or marked quadriparesis +5
									6|[local::at0033]  	-- Quadriplegia +6
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- Cerebellar
								value matches {
									0|[local::at0034], 	-- Normal +0
									1|[local::at0035], 	-- Abnormal signs without disability +1
									2|[local::at0036], 	-- Mild ataxia +2
									3|[local::at0037], 	-- Moderate truncal or limb ataxia +3
									4|[local::at0038], 	-- Severe ataxia, all limbs +4
									5|[local::at0039]  	-- Unable to perform coordinated movements due to ataxia +5
								}
							}
							ELEMENT[at0021] occurrences matches {0..1} matches {	-- Brainstem
								value matches {
									0|[local::at0040], 	-- Normal +0
									1|[local::at0041], 	-- Signs only +1
									2|[local::at0042], 	-- Moderate nystagmus or other mild disability +2
									3|[local::at0043], 	-- Severe nystagmus, marked extraocular weakness, or moderate disability of other cranial nerves +3
									4|[local::at0044], 	-- Marked dysarthria or other marked disability +4
									5|[local::at0045]  	-- Inability to swallow or speak +5
								}
							}
							ELEMENT[at0022] occurrences matches {0..1} matches {	-- Sensory
								value matches {
									0|[local::at0046], 	-- Normal +0
									1|[local::at0047], 	-- Vibration or figure-writing decrease only, in 1-2 limbs +1
									2|[local::at0048], 	-- Mild decrease in touch, pain or position sense, and/or moderate decrease in vibration in 1-2 limbs; or vibratory decrease alone in 3-4 limbs +2
									3|[local::at0049], 	-- Moderate decrease in touch, pain or position sense, and/or lost vibration in 1-2 limbs; or mild decrease in touch, pain and/or moderate decrease in all proprioceptive tests in 3-4 limbs +3
									4|[local::at0050], 	-- Marked decrease in touch or pain or loss of proprioception, alone or combined, in 1-2 limbs; or moderate decrease in touch, pain and/or severe proprioceptive decrease in >2 limbs +4
									5|[local::at0051], 	-- Loss of sensation in 1-2 limbs; or moderate decrease in touch, pain and/or loss of proprioception for most of the body below the head +5
									6|[local::at0052]  	-- Sensation essentially lost below the head +6
								}
							}
							ELEMENT[at0023] occurrences matches {0..1} matches {	-- Bowel and bladder
								value matches {
									0|[local::at0053], 	-- Normal +0
									1|[local::at0054], 	-- Mild urinary hesitancy, urgency, or retention +1
									2|[local::at0055], 	-- Moderate hesitancy, urgency, retention of bowel or bladder, or rare urinary incontinence +2
									3|[local::at0056], 	-- Frequent urinary incontinence +3
									4|[local::at0057], 	-- In need of almost constant catheterization +4
									5|[local::at0058], 	-- Loss of bladder function +5
									6|[local::at0059]  	-- Loss of bowel and bladder function+6
								}
							}
							ELEMENT[at0024] occurrences matches {0..1} matches {	-- Visual
								value matches {
									0|[local::at0060], 	-- 
									1|[local::at0061], 	-- Scotoma with visual acuity (corrected) better than 20/30 +1
									2|[local::at0062], 	-- Worse eye with scotoma with maximal visual acuity (corrected) of 20/30 to 20/59 +2
									3|[local::at0063], 	-- Worse eye with large scotoma, or moderate decrease in fields, but with maximal visual acuity (corrected) of 20/60 to 20/99 +3
									4|[local::at0064], 	-- Worse eye with marked decrease of fields and maximal visual acuity (corrected) of 20/100-20/200; grade 3 plus maximal acuity of better eye of ≤20/60 +4
									5|[local::at0065], 	-- Worse eye with maximal visual acuity (corrected) less than 20/200; grade 4 plus maximal acuity of better eye of 20/60 or less +5
									6|[local::at0066]  	-- Grade 5 plus maximal visual acuity of better eye of 20/60 or less +6
								}
							}
							ELEMENT[at0025] occurrences matches {0..1} matches {	-- Cerebral
								value matches {
									0|[local::at0067], 	-- Normal +0
									1|[local::at0068], 	-- Mood alteration only (Does not affect DSS score) +1
									2|[local::at0069], 	-- Mild decrease in mentation +2
									3|[local::at0070], 	-- Moderate decrease in mentation +3
									4|[local::at0071], 	-- Marked decrease in mentation (chronic brain syndrome - moderate) +4
									5|[local::at0072]  	-- Dementia or chronic brain syndrome - severe or incompetent
								}
							}
							ELEMENT[at0026] occurrences matches {0..1} matches {	-- Other
								value matches {
									0|[local::at0073], 	-- Normal +0
									1|[local::at0074]  	-- Any other neurologic findings attributed to MS +1
								}
							}
						}
					}
					ELEMENT[at0075] occurrences matches {0..1} matches {	-- Ambulation Score
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::380]>
								list = <
									["1"] = <
										units = <"1">
									>
								>
							>
						}
					}
					ELEMENT[at0076] occurrences matches {0..1} matches {	-- Functional System Score
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::380]>
								list = <
									["1"] = <
										units = <"1">
									>
								>
							>
						}
					}
					ELEMENT[at0085] occurrences matches {0..1} matches {	-- Functional System Score Interval
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0084] occurrences matches {0..1} matches {	-- No.of input
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0078] occurrences matches {0..1} matches {	-- No.of 0 in input
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
											>
										>
									>
								}
							}
							ELEMENT[at0079] occurrences matches {0..1} matches {	-- No.of 1 in input
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
											>
										>
									>
								}
							}
							ELEMENT[at0077] occurrences matches {0..1} matches {	-- No.of 2 in input
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
											>
										>
									>
								}
							}
							ELEMENT[at0080] occurrences matches {0..1} matches {	-- No.of 3 in input
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
											>
										>
									>
								}
							}
							ELEMENT[at0081] occurrences matches {0..1} matches {	-- No.of 4 in input
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
											>
										>
									>
								}
							}
							ELEMENT[at0082] occurrences matches {0..1} matches {	-- No.of 5 in input
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
											>
										>
									>
								}
							}
							ELEMENT[at0083] occurrences matches {0..1} matches {	-- No.of 6 in input
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
											>
										>
									>
								}
							}
						}
					}
				}
			}
		}
	}

