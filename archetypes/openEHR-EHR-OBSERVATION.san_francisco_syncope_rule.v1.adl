definition
	OBSERVATION[at0000] matches {	-- San francisco syncope rule
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- CHF History
										value matches {
											0|[local::at0021], 	-- False
											1|[local::at0022]  	-- True
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Hematocrit <30%
										value matches {
											0|[local::at0019], 	-- False
											1|[local::at0020]  	-- True
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- EKG abnormal
										value matches {
											0|[local::at0017], 	-- False
											1|[local::at0018]  	-- True
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Shortness of Breath
										value matches {
											0|[local::at0015], 	-- False
											1|[local::at0016]  	-- True
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Systolic BP <90 mmHg at Triage
										value matches {
											0|[local::at0013], 	-- False
											1|[local::at0014]  	-- True
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Any response positive?
										value matches {
											0|[local::at0011], 	-- False
											1|[local::at0012]  	-- True
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

