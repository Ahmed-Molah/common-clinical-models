archetype (adl_version=1.4)
	openEHR-EHR-OBSERVATION.abg_analyser.v1

concept
	[at0000]	-- Abg analyser
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Syeeda S Farruque">
		["organisation"] = <"Cambio Healthcare Systems">
		["date"] = <"2017-01-12">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"This tool calculates the AG (Anion gap) the corrected Anion Gap (cAG), the primary acid/base disturbance of blood PH and the compensatory - if any - reaction by utilising the various blood gas/electrolytes values found in a patient's record.

Increased Anion gap is measured in the presence of a metabolic acidosis with a corrected gap in the presence of albumin values.">
			use = <"To ascertain whether the patient's blood picture represents a metabolic or respiratory acidosis or alkalosis and any compensation going on, the PH, PaCO2 and bicarbonate are looked at:

1-Is the PH normal?
2-Is the PaCO2 normal?
3-Is the HCO3 normal?
4-Match the direction of difference of the PaCO2 or HCO3 with the PH
5-Does the PaCO2 or the HCO3 move in the opposite direction to the PH?

Normal ranges used:

PH: 7.35-7.45
PaCO2: 35-45mmHg (4.7-KPa)
HCO3-: 22-29mmol/L
Na: 135-145mmol/L
Cl: 95-105mmol/L">
			keywords = <"ABG analyser", "ph", "anion gap", "arterial blood gas analyser">
			misuse = <"Do not use in isolation of a thorough clinical evaluation of the patient's state">
			copyright = <"Â© Cambio Healthcare Systems">
		>
	>
	lifecycle_state = <"AuthorDraft">
	other_contributors = <>
	other_details = <
		["references"] = <"Ref. 1: Baillie JK. Simple, easily memorised 'rules of thumb' for the rapid assessment of physiological compensation for respiratory acid-base disorders. Thorax 2008;63:289-290 doi:10.1136/thx.2007.091223

Ref. 2: Kaufman DA. Interpretation of Arterial Blood Gases (ABGs). Written for Thoracic.org. Retrieved 12/9/2014.">
		["MD5-CAM-1.0.1"] = <"029C937E89864D310F4A28CFA3673B00">
	>

definition
	OBSERVATION[at0000] matches {	-- Abg analyser
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- PH acidotic?
										value matches {
											0|[local::at0007], 	-- No - normal 
											1|[local::at0008], 	-- No - alkalotic
											2|[local::at0009]  	-- Yes
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {	-- Acute/chronic
										value matches {
											0|[local::at0015], 	-- Acute
											1|[local::at0016]  	-- Chronic
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Albumin level
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"gm/l">
													>
													["2"] = <
														units = <"gm/dl">
													>
												>
											>
										}
									}
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Anion gap
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"mEq/l">
													>
													["2"] = <
														units = <"mmol/l">
													>
												>
											>
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Corrected AG
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"gm/l">
													>
													["2"] = <
														units = <"gm/dl">
													>
													["3"] = <
														units = <"mmol/l">
													>
												>
											>
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {	-- Respiratory acidosis
										value matches {
											0|[local::at0029], 	-- No
											1|[local::at0030]  	-- Primary Respiratory acidosis
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {	-- Respiratory alkalosis
										value matches {
											0|[local::at0031], 	-- No
											1|[local::at0032]  	-- Primary Respiratory alkalosis
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {	-- Metabolic acidosis
										value matches {
											0|[local::at0033], 	-- No
											1|[local::at0034]  	-- Primary Metabolic acidosis
										}
									}
									ELEMENT[at0028] occurrences matches {0..1} matches {	-- Metabolic alkalosis
										value matches {
											0|[local::at0035], 	-- No
											1|[local::at0036]  	-- Primary Metabolic alkalosis
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {	-- Final analysis
										value matches {
											0|[local::at0039], 	-- N/A
											1|[local::at0040], 	-- Compenatory respiratory acidosis
											2|[local::at0041], 	-- Compensatory respiratory alkalosis
											3|[local::at0042], 	-- Compensatory metabolic acidosis
											4|[local::at0043], 	-- Compensatory metabolic alkalosis
											5|[local::at0044], 	-- Uncompensated
											6|[local::at0048]  	-- Mixed disturbance
										}
									}
									ELEMENT[at0045] occurrences matches {0..1} matches {	-- Associated Anion Gap?
										value matches {
											0|[local::at0046], 	-- No AG
											1|[local::at0047]  	-- AG present
										}
									}
									ELEMENT[at0049] occurrences matches {0..1} matches {	-- Delta gap if AG present
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"1">
													>
												>
											>
										}
									}
									ELEMENT[at0050] occurrences matches {0..1} matches {	-- Delta ratio assessment
										value matches {
											0|[local::at0051], 	-- NA
											1|[local::at0052], 	-- <0.4
											2|[local::at0053], 	-- <1
											3|[local::at0054], 	-- 1-2
											4|[local::at0055]  	-- >2
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {	-- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

