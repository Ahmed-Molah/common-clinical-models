		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the result of the Risk of Paradoxical Embolism (RoPE) Score assessment. ">
			use = <"Use to record the result of the Risk of Paradoxical Embolism (RoPE) Score assessment in patients with cryptogenic stroke found to have PFO. ">
