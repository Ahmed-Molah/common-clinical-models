definition
	EVALUATION[at0000] matches {	-- Rope score
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- PFO-attributable fraction (95% CI)
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0004, 	-- 0% (0–4)
									at0005, 	-- 38% (25–48)
									at0006, 	-- 34% (21–45)
									at0007, 	-- 62% (54–68)
									at0008, 	-- 72% (66–76)
									at0009, 	-- 84% (79–87)
									at0010]	-- 88% (83–91)
								}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Estimated stroke/TIA recurrence at 2 years
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0011, 	-- 20% (12–28)
									at0012, 	-- 12% (6–18)
									at0013, 	-- 7% (3–11)
									at0014, 	-- 8% (4–12)
									at0015, 	-- 6% (2–10)
									at0017]	-- 2% (0–4)
								}
							}
						}
					}
				}
			}
		}
	}

