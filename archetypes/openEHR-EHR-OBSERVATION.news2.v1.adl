definition
	OBSERVATION[at0000] matches {	-- National Early Warning Score (NEWS) 2
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(hu)
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- *Any event(hu)
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Respiratory rate
										value matches {
											0|[local::at0005], 	-- 12-20
											1|[local::at0006], 	-- 9-11
											2|[local::at0007], 	-- 21-24
											3|[local::at0008]  	-- ≤8 or ≥25
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Hypercapnic respiratory failure
										value matches {
											0|[local::at0010], 	-- No
											1|[local::at0011]  	-- Yes
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- SpO2
										value matches {
											0|[local::at0013], 	-- ≥96%, hypercapnic respiratory failure 88-92%, ≥93% on room air
											1|[local::at0014], 	-- 94-95%, hypercapnic respiratory failure 86-87% or 95-96% on supplemental oxygen
											2|[local::at0015], 	-- 92-93%, hypercapnic respiratory failure 84-85% or 93-94% on supplemental oxygen
											3|[local::at0016]  	-- ≤91%, hypercapnic respiratory failure ≤83% or ≥97% on supplemental oxygen
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Supplemental oxygen
										value matches {
											0|[local::at0018], 	-- Room air
											2|[local::at0019]  	-- Supplemental oxygen
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {	-- Temperature
										value matches {
											0|[local::at0021], 	-- 36.1-38.0°C
											1|[local::at0022], 	-- 35.1-36.0°C or 38.1-39.0°C
											2|[local::at0023], 	-- ≥39.1°C
											3|[local::at0024]  	-- ≤35.0°C
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {	-- Systolic blood pressure
										value matches {
											0|[local::at0026], 	-- 111-219 mmHg
											1|[local::at0027], 	-- 101-110 mmHg
											2|[local::at0028], 	-- 91-100 mmHg
											3|[local::at0029]  	-- ≤90 or ≥220 mmHg
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {	-- Pulse
										value matches {
											0|[local::at0031], 	-- 51-90 bpm
											1|[local::at0032], 	-- 41-50 or 91-110 bpm
											2|[local::at0033], 	-- 111-130 bpm
											3|[local::at0034]  	-- ≤40 or ≥131 bpm
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {	-- Consciousness
										value matches {
											0|[local::at0036], 	-- Alert
											3|[local::at0037]  	-- New-onset confusion (or disorientation/agitation), responds to voice, responds to pain, or unresponsive
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {	-- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..20|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

