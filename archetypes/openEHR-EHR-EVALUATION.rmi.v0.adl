definition
	EVALUATION[at0000] matches {	-- Risk of malignancy index (RMI)
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0002] occurrences matches {0..1} matches {	-- Ultrasound features
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0003] occurrences matches {0..1} matches {	-- Multiloculated cysts
								value matches {
									0|[local::at0004], 	-- No
									1|[local::at0005]  	-- Yes
								}
							}
							ELEMENT[at0006] occurrences matches {0..1} matches {	-- Solid areas
								value matches {
									0|[local::at0007], 	-- No
									1|[local::at0008]  	-- Yes
								}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {	-- Bilateral lesions
								value matches {
									0|[local::at0010], 	-- No
									1|[local::at0011]  	-- Yes
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {	-- Ascites
								value matches {
									0|[local::at0013], 	-- No
									1|[local::at0014]  	-- Yes
								}
							}
							ELEMENT[at0015] occurrences matches {0..1} matches {	-- Intra-abdominal metastases
								value matches {
									0|[local::at0016], 	-- No
									1|[local::at0017]  	-- Yes
								}
							}
							ELEMENT[at0026] occurrences matches {0..1} matches {	-- Ultrasound features score
								value matches {
									DV_COUNT matches {*}
								}
							}
						}
					}
					CLUSTER[at0018] occurrences matches {0..1} matches {	-- Other features
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0019] occurrences matches {0..1} matches {	-- Menopausal status
								value matches {
									1|[local::at0020], 	-- Premenopausal or perimenopausal
									3|[local::at0021]  	-- Postmenopausal
								}
							}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Risk of malignancy
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0023, 	-- Low
									at0024]	-- High
								}
							}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- RMI
						value matches {
							DV_COUNT matches {*}
						}
					}
				}
			}
		}
	}

