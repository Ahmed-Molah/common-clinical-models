definition
	OBSERVATION[at0000] matches {	-- Wells' Criteria for Pulmonary Embolism
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Clinical signs and symptoms of DVT
										value matches {
											0|[local::at0013], 	-- No
											6|[local::at0014]  	-- Yes
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- Heart rate >100 beats/min
										value matches {
											0|[local::at0013], 	-- No
											3|[local::at0014]  	-- Yes
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Immobilization ≥3 days OR surgery in the previous 4 weeks
										value matches {
											0|[local::at0013], 	-- No
											3|[local::at0014]  	-- Yes
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Previous, objectively diagnosed PE or DVT
										value matches {
											0|[local::at0013], 	-- No
											3|[local::at0014]  	-- Yes
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Hemoptysis
										value matches {
											0|[local::at0013], 	-- No
											2|[local::at0014]  	-- Yes
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Malignancy w/ treatment within 6 months or palliative
										value matches {
											0|[local::at0013], 	-- No
											2|[local::at0014]  	-- Yes
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Is PE the number 1 diagnosis OR equally likely
										value matches {
											0|[local::at0013], 	-- No
											6|[local::at0014]  	-- Yes
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- Total score
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"1">
														magnitude = <|0.0..12.5|>
														precision = <|1|>
													>
												>
											>
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Comments
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

