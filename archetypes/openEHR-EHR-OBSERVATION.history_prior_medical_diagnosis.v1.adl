										}
									}
									ELEMENT[at0054] occurrences matches {0..1} matches {	-- Endokrin sjukdom
										value matches {
											0|[local::at0055], 	-- Nej
											1|[local::at0056]  	-- Ja
										}
									}
									ELEMENT[at0057] occurrences matches {0..1} matches {	-- Koagulationsrubbning
										value matches {
											0|[local::at0059], 	-- Nej
											1|[local::at0060]  	-- Ja
										}
									}
									ELEMENT[at0061] occurrences matches {0..1} matches {	-- Blödning (ospecifik)
										value matches {
											0|[local::at0063], 	-- Nej
											1|[local::at0064]  	-- Ja
										}
									}
									CLUSTER[at0062] occurrences matches {0..1} matches {*}
									CLUSTER[at0058] occurrences matches {0..1} matches {*}
									CLUSTER[at0053] occurrences matches {0..1} matches {*}
									CLUSTER[at0046] occurrences matches {0..1} matches {*}
									CLUSTER[at0019] occurrences matches {0..1} matches {	-- Specifik kardiovaskulär sjukdom
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- Hypertoni
												value matches {
													0|[local::at0011], 	-- Nej
													1|[local::at0012]  	-- Ja
												}
											}
											ELEMENT[at0031] occurrences matches {0..1} matches {	-- Stroke/cerebrovaskulär lesion
												value matches {
													0|[local::at0035], 	-- Nej
													1|[local::at0036]  	-- Ja
												}
											}
											ELEMENT[at0032] occurrences matches {0..1} matches {	-- Transitorisk ischemisk attack (TIA)
												value matches {
													0|[local::at0033], 	-- Nej
													1|[local::at0034]  	-- Ja
												}
											}
											ELEMENT[at0037] occurrences matches {0..1} matches {	-- Förmaksflimmer
												value matches {
													0|[local::at0038], 	-- Nej
													1|[local::at0039]  	-- Ja
												}
											}
											ELEMENT[at0043] occurrences matches {0..1} matches {	-- Hjärtsvikt (ospecifik)
												value matches {
													0|[local::at0044], 	-- Nej
													1|[local::at0045]  	-- Ja
												}
											}
											ELEMENT[at0005] occurrences matches {0..1} matches {	-- Hjärtsvikt (specifik)
												value matches {
													0|[local::at0009], 	-- Nej
													1|[local::at0010]  	-- Ja
												}
											}
										}
									}
									CLUSTER[at0020] occurrences matches {0..1} matches {	-- Specifik lungsjukdom
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0047] occurrences matches {0..1} matches {	-- Kronisk obstruktiv lungsjukdom (KOL)
												value matches {
													0|[local::at0048], 	-- Nej
													1|[local::at0049]  	-- Ja
												}
											}
										}
									}
									CLUSTER[at0021] occurrences matches {0..1} matches {	-- Specifik endokrin sjukdom
