definition
	OBSERVATION[at0000] matches {	-- Medicinsk historik
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(en)
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- *Any event(en)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Tree(en)
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- Kardiovaskulär sjukdom
										value matches {
											0|[local::at0024], 	-- Nej
											1|[local::at0025]  	-- Ja
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Lungsjukdom
										value matches {
											0|[local::at0027], 	-- Nej
											1|[local::at0028]  	-- Ja
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {	-- Reumatisk eller annan autoimmun sjukdom
										value matches {
											0|[local::at0029], 	-- Nej
											1|[local::at0030]  	-- Ja
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {	-- Leversjukdom
										value matches {
											0|[local::at0041], 	-- Nej
