concept
	[at0000]	-- FIGO Staging for Ovarian Cancer (2014)
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Henrik Honkanen">
		["email"] = <"henrik.honkanen@stud.ki.se">
		["organisation"] = <"Cambio CDS">
		["date"] = <"2020-07-14">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Used to stages cancer of the ovary, fallopian tube, and peritoneum.">
			use = <"Use postoperatively in patients with histologically-proven ovarian cancer (surgical pathology is needed in order to stage). Stages range from I-IV.">
			keywords = <"FIGO staging", "ovarian cancer", "fallopian tube", "peritoneum">
			misuse = <"">
			copyright = <"Cambio CDS">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
	other_details = <
		["references"] = <"1. Prat J, FIGO Committee on Gynecologic Oncology. Staging classification for cancer of the ovary, fallopian tube, and peritoneum. International Journal of Gynecology & Obstetrics. 2014 Jan;124(1):1-5.">
		["current_contact"] = <"Henrik Honkanen, Cambio CDS<henrik.honkanen@stud.ki.se>">
		["MD5-CAM-1.0.1"] = <"1706F1A6CC0C7ED72BC728663EA12DEE">
	>

definition
	EVALUATION[at0000] matches {	-- FIGO Staging for Ovarian Cancer (2014)
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0006] occurrences matches {0..1} matches {	-- FIGO stage
						items cardinality matches {1..*; unordered} matches {
							CLUSTER[at0007] occurrences matches {0..1} matches {	-- Stage I: Tumor confined to ovaries
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0002] occurrences matches {0..1} matches {	-- Stage I: Tumor confined to ovaries
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0008, 	-- IA - Tumor limited to 1 ovary, capsule intact, no tumor on surface, negative washings
													at0009, 	-- IB - Tumor involves both ovaries otherwise like IA
													at0010]	-- IC - Tumor limited to 1 or both ovaries
												}
											}
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- IC - Tumor limited to 1 or both ovaries
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0012, 	-- IC1 - Surgical spill
													at0013, 	-- IC2 - Capsule rupture before surgery or tumor on ovarian surface
													at0014]	-- IC3 - Malignant cells in the ascites or peritoneal washings
												}
											}
										}
									}
								}
							}
							CLUSTER[at0015] occurrences matches {0..1} matches {	-- Stage II: Tumor involves 1 or both ovaries with pelvic extension (below the pelvic brim) or primary peritoneal cancer
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0003] occurrences matches {0..1} matches {	-- Stage II: Tumor involves 1 or both ovaries with pelvic extension (below the pelvic brim) or primary peritoneal cancer
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0018, 	-- IIA - Extension and/or implant on uterus and/or fallopian tubes
													at0019]	-- IIB - Extension to other pelvic intraperitoneal tissues
												}
											}
										}
									}
								}
							}
							CLUSTER[at0016] occurrences matches {0..1} matches {	-- Stage III: Tumor involves 1 or both ovaries with cytologically or histologically confirmed spread to the peritoneum outside the pelvis and/or metastasis to the retroperitoneal lymph nodes
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Stage III: Tumor involves 1 or both ovaries with cytologically or histologically confirmed spread to the peritoneum outside the pelvis and/or metastasis to the retroperitoneal lymph nodes
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0021, 	-- IIIA - Positive retroperitoneal lymph nodes and/or microscopic metastasis beyond the pelvis
													at0027, 	-- IIIB - Macroscopic, extrapelvic, peritoneal metastasis ≤2 cm ± positive retroperitoneal lymph nodes; includes extension to capsule of liver/spleen
													at0028]	-- IIIC - Macroscopic, extrapelvic, peritoneal metastasis >2 cm ± positive retroperitoneal lymph nodes; includes extension to capsule of liver/spleen
												}
											}
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {	-- IIIA Positive retroperitoneal lymph nodes and/or microscopic metastasis beyond the pelvis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0022, 	-- IIIA1 - Positive retroperitoneal lymph nodes only
													at0026]	-- IIIA2 - Microscopic, extrapelvic (above the brim) peritoneal involvement ± positive retroperitoneal lymph nodes
												}
											}
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {	-- IIIA1 - Positive retroperitoneal lymph nodes only
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0024, 	-- IIIA1(i) - Metastasis ≤10 mm
													at0025]	-- IIIA1(ii) - Metastasis >10 mm
												}
											}
										}
									}
								}
							}
							CLUSTER[at0017] occurrences matches {0..1} matches {	-- Stage IV: Distant metastasis excluding peritoneal metastasis
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Stage IV: Distant metastasis excluding peritoneal metastasis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0029, 	-- IVA - Pleural effusion with positive cytology
													at0030]	-- IVB - Hepatic and/or splenic parenchymal metastasis, metastasis to extra-abdominal organs (including inguinal lymph nodes and lymph nodes outside of the abdominal cavity)
												}
											}
										}
									}
								}
							}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Stage Interpretation
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0032, 	-- Stage - IA
									at0033, 	-- Stage - IB
									at0034, 	-- Stage - IC1
									at0035, 	-- Stage - IC2
									at0036, 	-- Stage - IC3
									at0037, 	-- Stage - IIA
									at0038, 	-- Stage - IIB
									at0039, 	-- Stage - IIIA1(i)
									at0040, 	-- Stage - IIIA1(ii)
									at0041, 	-- Stage - IIIA2
									at0042, 	-- Stage - IIIB
									at0043, 	-- Stage - IIIC
									at0044, 	-- Stage - IVA
									at0045]	-- Stage - IVB
								}
							}
						}
					}
				}
			}
		}
	}

