	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To detect unmeasured compounds in the serum. Osmolal gaps (difference between measured and expected) of >10 can be caused by toxic alcohols. ">
			keywords = <"serum osmolarity, osmolal gap, serum osmolality", ...>
			copyright = <"Cambio CDS">
			use = <"Purssell et al equation for calculating serum osmolality with US units: Serum osm [mOsm/kg] = (2 Ã— Na + (BUN / 2.8) + (glucose / 18) + (ethanol / 3.7).
For SI units, the values do not need to be divided by their molecular weight.">
		>
	>
