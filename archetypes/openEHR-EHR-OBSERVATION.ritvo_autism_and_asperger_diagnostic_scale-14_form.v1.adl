	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To screen for ASD (Autism Spectrum Disorder) in adult psychiatric outpatients by using a 14 item self-evaluation questionnaire.">
			use = <"To be used in adult psychiatric outpatients. This is a shorten version of the 80 item Ritvo Autism and Asperger Diagnostic Scale-Revised (RAADS-R). The score range from a minimum of 0 to a maximum of 42. Three main subdomains are included: Mentalizing deficits, Social anxiety and Sensory reactivity.Three scoring categories are possible: 31 & up Autism found; 14 – 30 Autism likely; 0 – 13 No autism symptoms.">
			keywords = <"psychiatry", "ASD", "autism spectrum disorder", "adult psychiatric outpatiens", "patient questionnaire">
			misuse = <"Do not use for other than to screen adult psychiatric outpatients for ASD. Do not use to register results for the 80 points RAADS score.">
			copyright = <"Cambio Healthcare Systems">
		>
		["sv"] = <
			language = <[ISO_639-1::sv]>
			purpose = <"Avsett för diagnostisering av autism, ASD (Autism Spectrum Disorder), hos vuxna genom ett 14 frågor självskattningsformulär.">
			use = <"Används endast för vuxna patienter. Förkortat version av 80 st Ritvo Autism and Asperger Diagnostic Scale-Revised (RAADS-R). Skalan varierar mellan 0(min) och 42(max). RAADS-14 screen innehåller tre subskalor: Mentaliseringssvårigheter, Social ångest och Sensorisk reaktivitet. Totalskalan > 31- Autism; 14-30- Möjligt Autism; 0-13- Inga Autism Symptom.">
			keywords = <"psykiatri", "ASD", "autismspektrumstörning", "vuxna psykistri patienter", "patientenkät">
			misuse = <"Används ensast för screening av autismspektrumstörning hos vuxna. Används ej för att registrera resultater för RAADS skalan baserad på 80 poäng. ">
			copyright = <"Cambio Healthcare Systems">
		>
	>
