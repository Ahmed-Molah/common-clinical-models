definition
	OBSERVATION[at0000] matches {	-- Psyk raads 14 form
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Review Date/Time
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- Mentalizing Deficits
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0007] occurrences matches {0..1} matches {	-- It is difficult for me to understand how other people are feeling when we are talking.
												value matches {
													0|[local::at0008], 	-- Never true
													1|[local::at0009], 	-- True only when I was younger than 16
													2|[local::at0010], 	-- True only now
													3|[local::at0011]  	-- True now and when I was young
												}
											}
											ELEMENT[at0012] occurrences matches {0..1} matches {	-- It is difficult to figure out what other people expect of me.
												value matches {
													0|[local::at0013], 	-- Never true
													1|[local::at0014], 	-- True only when I was younger than 16
													2|[local::at0015], 	-- True only now
													3|[local::at0017]  	-- True now and when I was young
												}
											}
											ELEMENT[at0016] occurrences matches {0..1} matches {	-- When talking to someone, I have a hard time telling when it is my turn to talk or to listen.
												value matches {
													0|[local::at0018], 	-- Never true
													1|[local::at0019], 	-- True only when I was younger than 16
													2|[local::at0020], 	-- True only now
													3|[local::at0022]  	-- True now and when I was young
												}
											}
											ELEMENT[at0021] occurrences matches {0..1} matches {	-- It can be very hard to read someone’s face, hand, and body movements when we are talking.
												value matches {
													0|[local::at0023], 	-- Never true
													1|[local::at0024], 	-- True only when I was younger than 16
													2|[local::at0025], 	-- True only now
													3|[local::at0027]  	-- True now and when I was young
												}
											}
											ELEMENT[at0026] occurrences matches {0..1} matches {	-- I focus on details rather than the overall idea.
												value matches {
													0|[local::at0028], 	-- Never true
													1|[local::at0029], 	-- True only when I was younger than 16
													2|[local::at0030], 	-- True only now
													3|[local::at0031]  	-- True now and when I was young
												}
											}
											ELEMENT[at0032] occurrences matches {0..1} matches {	-- I take things too literally, so I often miss what people are trying to say.
												value matches {
													0|[local::at0033], 	-- Never true
													1|[local::at0034], 	-- True only when I was younger than 16
													2|[local::at0035], 	-- True only now
													3|[local::at0037]  	-- True now and when I was young
												}
											}
											ELEMENT[at0036] occurrences matches {0..1} matches {	-- I get extremely upset when the way I like to do things is suddenly changed
												value matches {
													0|[local::at0038], 	-- Never true
													1|[local::at0039], 	-- True only when I was younger than 16
													2|[local::at0040], 	-- True only now
													3|[local::at0042]  	-- True now and when I was young
												}
											}
										}
									}
									CLUSTER[at0041] occurrences matches {0..1} matches {	-- Social Anxiety
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0043] occurrences matches {0..1} matches {	-- It is very difficult for me to work and function in groups.
												value matches {
													0|[local::at0044], 	-- Never true
													1|[local::at0045], 	-- True only when I was younger than 16
													2|[local::at0046], 	-- True only now
													3|[local::at0048]  	-- True now and when I was young
												}
											}
											ELEMENT[at0047] occurrences matches {0..1} matches {	-- I often don’t know how to act in social situations.
												value matches {
													0|[local::at0049], 	-- Never true
													1|[local::at0050], 	-- True only when I was younger than 16
													2|[local::at0051], 	-- True only now
													3|[local::at0053]  	-- True now and when I was young
												}
											}
											ELEMENT[at0052] occurrences matches {0..1} matches {	-- I can chat and make small talk with people.
												value matches {
													0|[local::at0054], 	-- True now and when I was young
													1|[local::at0055], 	-- True only now
													2|[local::at0056], 	-- True only when I was younger than 16
													3|[local::at0058]  	-- Never true
												}
											}
											ELEMENT[at0057] occurrences matches {0..1} matches {	-- How to make friends and socialize is a mystery to me.
												value matches {
													0|[local::at0059], 	-- Never true
													1|[local::at0060], 	-- True only when I was younger than 16
													2|[local::at0061], 	-- True only now
													3|[local::at0062]  	-- True now and when I was young
												}
											}
										}
									}
									CLUSTER[at0063] occurrences matches {0..1} matches {	-- Sensory Reactivity
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0064] occurrences matches {0..1} matches {	-- Some ordinary textures that do not bother others feel very offensive when they touch my skin.
												value matches {
													0|[local::at0065], 	-- Never true
													1|[local::at0066], 	-- True only when I was younger than 16
													2|[local::at0067], 	-- True only now
													3|[local::at0069]  	-- True now and when I was young
												}
											}
											ELEMENT[at0068] occurrences matches {0..1} matches {	-- When I feel overwhelmed by my senses, I have to isolate myself to shut them down.
												value matches {
													0|[local::at0070], 	-- Never true
													1|[local::at0071], 	-- True only when I was younger than 16
													2|[local::at0072], 	-- True only now
													3|[local::at0074]  	-- True now and when I was young
												}
											}
											ELEMENT[at0073] occurrences matches {0..1} matches {	-- Sometimes I have to cover my ears to block out painful noises (like vacuum cleaners or people talking too much or too loudly).
												value matches {
													0|[local::at0075], 	-- Never true
													1|[local::at0076], 	-- True only when I was younger than 16
													2|[local::at0077], 	-- True only now
													3|[local::at0078]  	-- True now and when I was young
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

