  </div>
  <div class="row" title="{{${total_score_def}.description}}" id="displayresult">
    <div class="medium-5 columns">
      <h5><b>{{${total_score_def}.name}}</b></h5>
    </div>
    <div class="medium-7 columns">
      <h5 id="fix">{{${total_score_cds_data}.value.magnitude}}</h5>
    </div>
  </div>
