<script> 
angular.module('cds_app').controller('Bishop_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	$scope.${dilation_ehr_data}.value.selection = $scope.findMax($scope.${dilation_def});
	$scope.${effacement_ehr_data}.value.selection = $scope.findMax($scope.${effacement_def});
	$scope.${fetal_station_ehr_data}.value.selection = $scope.findMax($scope.${fetal_station_def});
	$scope.${position_ehr_data}.value.selection = $scope.findMax($scope.${position_def});
	$scope.${consistency_ehr_data}.value.selection = $scope.findMax($scope.${consistency_def});
	$scope.execute();
  });
  $scope.findMax = function (element) {
				var array = [];
				for(selection in element.selections) {
					array.push(selection);
				}
				return (function() {
					var max = Number.MIN_VALUE;
					var result;
					for (var i=0; i<array.length; i++) {
						var value = parseFloat(array[i].split("|")[0]);
						if (value >= max) {
							max = value;
							result = array[i];
						}
					}
					return result;
				})(array);
			};
});
</script>

