  "decisionSupportViewDefinitions": {
    "sv": {
      "name": "Beräkning av LDL",
      "description": "LDL-kolesterol utgör en del av en fullständig lipidprofil. Friedewalds formel möjliggör beräkning av en patients LDL-kolesterol enligt:  (Totalt kolesterol - HDL kolesterol - [Triglycerider/5]).\n"
    },
    "en": {
      "name": "LDL Calculated",
      "description": "This guide calculates the LDL cholesterol level of an individual. LDL cholesterol is part of a full lipid profile."
    }
  },
  "resourceDescription": {
    "originalAuthor": {
      "email": "info@cambio.se",
      "organisation": "Cambio Healthcare Systems",
      "name": "Eneimi Allwell-Brown",
      "date": "2016-09-29"
