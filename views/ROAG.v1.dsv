<script>
  angular.module('cds_app').controller('Roag_ctrl', function($scope){
		$scope.checked = {};
		$scope.referral={};
		$scope.$on('json_loaded', function(){
		});
	
		$scope.validateAction = function (param, id) {
			if (param === 2)
				$scope.checked[id] = true;
			if (param ===3){
				$scope.checked[id] = true;
				$scope.referral[id] = true;
			}

    	$scope.execute();
  	};
	});
		
</script>
