<script>
angular.module('cds_app').controller('MNA-SF_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
    $scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
    $scope.${event_time2_ehr_data}.value.value = new Date().toISOString();
    $scope.${height_length_ehr_data}.value.units = $scope.${height_length_def}.units[1];
    $scope.${weight_ehr_data}.value.units = $scope.${weight_def}.units[1];
	$scope.${has_food_intake_declined_over_the_past_3_months_due_to_loss_of_appetite__digestive_problems__chewing_or_swallowing_difficulties__ehr_data}.value.selection = $scope.findMin($scope.${has_food_intake_declined_over_the_past_3_months_due_to_loss_of_appetite__digestive_problems__chewing_or_swallowing_difficulties__def});
	$scope.${weight_loss_during_the_last_3_months_ehr_data}.value.selection = $scope.findMin($scope.${weight_loss_during_the_last_3_months_def});
	$scope.${mobility_ehr_data}.value.selection = $scope.findMin($scope.${mobility_def});
	$scope.${has_suffered_psychological_stress_or_acute_disease_in_the_past_3_months__ehr_data}.value.selection = $scope.findMin($scope.${has_suffered_psychological_stress_or_acute_disease_in_the_past_3_months__def});
	$scope.${neuropsychological_problems_ehr_data}.value.selection = $scope.findMin($scope.${neuropsychological_problems_def});
	$scope.execute();
	
  });
  $scope.findMin = function (element) {
        var array = []
        for  (selection in element.selections) {
          array.push(selection);
        }
        return (function() {
          var min = 0;
          var result;
          for (i=0; i<array.length; i++) {
            if (array[i].charAt(0) <= min) {
              result = array[i];
            }
          }
          return result;
        })(array);
      };
  $scope.updateCC = function () {
    document.getElementById("cbox").checked === true ? $scope.checked = true : $scope.checked = false;
    $scope.execute();
  };
  $scope.$on('executeEnd', function() {
    $scope.firedRules.filter(function(rule) {
      if (rule.gtCode === 'gt0033' || rule.gtCode === 'gt0047' ) {
        $scope.bmi_cds = Math.round($scope.${body_mass_index_cds_data}.value.magnitude);
      }
    });
  });
});
</script>

