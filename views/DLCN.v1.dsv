    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time2_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time3_ehr_data}.value.value = new Date().toISOString();
    $scope.${ldl_cholesterol1_ehr_data}.value.units = $scope.${ldl_cholesterol1_def}.units[0];
    $scope.${birthdate_ehr_data}.value.selection = $scope.findMin($scope.${birthdate_def});
	$scope.${gender_ehr_data}.value.selection = $scope.findMin($scope.${gender_def});
	$scope.${coronary_heart_disease_ehr_data}.value.selection = $scope.findMin($scope.${coronary_heart_disease_def});
	$scope.${peripheral_vascular_disease_ehr_data}.value.selection = $scope.findMin($scope.${peripheral_vascular_disease_def});
	$scope.${cerebrovascular_disease_ehr_data}.value.selection = $scope.findMin($scope.${cerebrovascular_disease_def});
	$scope.${finding_of_tendon_xanthoma__ehr_data}.value.selection = $scope.findMin($scope.${finding_of_tendon_xanthoma__def});
