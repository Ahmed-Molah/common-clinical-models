	$scope.${hx_of_illegal_drug_abuse_ehr_data}.value.selection = $scope.findMin($scope.${hx_of_illegal_drug_abuse_def});
	$scope.${hx_of_prescription_drug_use_ehr_data}.value.selection = $scope.findMin($scope.${hx_of_prescription_drug_use_def});
	$scope.${fhx_alcohol_abuse_ehr_data}.value.selection = $scope.findMin($scope.${fhx_alcohol_abuse_def});
	$scope.${fhx_illegal_drug_abuse_ehr_data}.value.selection = $scope.findMin($scope.${fhx_illegal_drug_abuse_def});
