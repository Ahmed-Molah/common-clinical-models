  }
  h2 {
    text-align: left;
    font-weight: 300;
    font-size: 20px;
  }
  h3 {
    text-align: left;
    font-size: 14px;
    font-style: italic;
    font-weight: bold;
  }
  select, label:not(#h-fix) {
    margin-bottom: 10px;
  }
  .header {
    margin: 2em;
  }
  .wrapper1{
    text-align: center;
    margin-bottom: 1em;
  }
  .wrapper2{
    font-style: italic;
    max-width: 55%;
    margin: auto;
  }
  .card {
    background-color: #FFFAFA;
    border: 1px solid #99a6b2;
    border-width: thin;
    box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
    margin: 10px auto 0 auto;
