angular.module('cds_app').controller('ASSIGN_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
    $scope.${event_time2_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time3_ehr_data}.value.value = new Date().toISOString();
	$scope.${does_the_subject_have_rheumatoid_arthritis__ehr_data}.value.selection = $scope.findMin($scope.${does_the_subject_have_rheumatoid_arthritis__def});
	$scope.${is_the_subject_diabetic__ehr_data}.value.selection = $scope.findMin($scope.${is_the_subject_diabetic__def});
	$scope.${is_there_a_family_history_of_stroke_chd__ehr_data}.value.selection = $scope.findMin($scope.${is_there_a_family_history_of_stroke_chd__def});
    $scope.${number_of_cigarettes_smoked_daily_ehr_data}.value.units = $scope.${number_of_cigarettes_smoked_daily_def}.units[0];
	$scope.${simd_ehr_data}.value.units = $scope.${simd_def}.units[0];
	$scope.${age_ehr_data}.value.units = $scope.${age_def}.units[0];
	$scope.${gender_ehr_data}.value.selection = $scope.findMin($scope.${gender_def});
	$scope.${systolic_ehr_data}.value.units = $scope.${systolic_def}.units[0];
	$scope.${hdl_cholesterol_ehr_data}.value.units = $scope.${hdl_cholesterol_def}.units[1];
	$scope.${total_cholesterol_ehr_data}.value.units = $scope.${total_cholesterol_def}.units[1];
  });
});
</script>

