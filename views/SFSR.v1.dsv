    "sv": {
      "name": "San Francisco Syncope Rule",
      "description": "Identifiering av patienter med hög risk för synkope"
    },
    "en": {
      "name": "San Francisco Syncope Rule",
      "description": "Screening for patients with high risk of syncope"
    }
  },
  "resourceDescription": {
    "originalAuthor": {
      "date": "2017-03-03",
