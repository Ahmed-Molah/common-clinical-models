    </label>
  </div>
  <div class="row collapse" title="{{${diabetes_def}.description}}">
    <div class="medium-6 columns">
      <label>{{${diabetes_def}.name}}</label>
    </div>
    <label class="medium-3 columns" ng-repeat="(key, desc) in ${diabetes_def}.selections">
