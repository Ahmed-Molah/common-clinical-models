    <script>
    angular.module('cds_app').controller('Wells_Ctrl', function($scope, $window){
      $scope.$on('json_loaded', function(){
        $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
        $scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
        $scope.${d_dimer_value_ehr_data}.value.units = $scope.${d_dimer_value_def}.units[0];
    	  $scope.hideUltrasound=true; <!-- Hide ultrasound from start -->
      });
      $scope.$on('executeEnd', function() {
      	$scope.highRisk=false;
        $scope.hideDdimer=false;
        $scope.hideUltrasound=true;
        if ($scope.${total_score_cds_data}.value.magnitude >= 2 ) { <!-- check if total score is greater than two -->
          $scope.hideUltrasound=false;
          $scope.hideDdimer=true;
          $scope.highRisk=true;
        }
        if ($scope.${total_score_cds_data}.value.magnitude < 2 && $scope.${d_dimer_value_ehr_data}.value.magnitude >= 0.25 ) { <!-- check if d-dimer is elevated -->
          $scope.hideUltrasound=false;
        }
      });
      $scope.signForm = function() {
      $window.cosmic.extension.cds.process('sign');
      }
    });

    </script>

