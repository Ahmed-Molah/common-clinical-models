<body ng-controller="cds_controller">
<#assign app_help_tag = "def.guides['NYHA.v1'].concept">
<#assign references = "def.terms['vt0001']"/>
<#assign nyha_questionnaire_entry_ehr_data = "data.ehr['openEHR-EHR-OBSERVATION.nyha_questionnaire.v1']">
<#assign nyha_questionnaire_entry_def = "def.entries['openEHR-EHR-OBSERVATION.nyha_questionnaire.v1']">
<#assign what_is_the_objective_assessment___def = nyha_questionnaire_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0004}']">
<#assign what_are_the_patient_symptoms___def = nyha_questionnaire_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0005}']">
<#assign event_time_def = nyha_questionnaire_entry_def + ".elements['/data/events/time']">
<#assign what_is_the_objective_assessment___ehr_data = nyha_questionnaire_entry_ehr_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0004}']">
<#assign what_are_the_patient_symptoms___ehr_data = nyha_questionnaire_entry_ehr_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0005}']">
<#assign event_time_ehr_data = nyha_questionnaire_entry_ehr_data + "[0].elements['/data/events/time']">
<#assign nyha_classification_entry_cds_data = "data.cds['openEHR-EHR-EVALUATION.nyha_classification.v1']">
<#assign nyha_classification_entry_def = "def.entries['openEHR-EHR-EVALUATION.nyha_classification.v1']">
<#assign objective_assessment_def = nyha_classification_entry_def + ".elements['/data{at0001}/items{at0003}']">
<#assign patient_symptoms_def = nyha_classification_entry_def + ".elements['/data{at0001}/items{at0002}']">
<#assign objective_assessment_cds_data = nyha_classification_entry_cds_data + "[0].elements['/data{at0001}/items{at0003}']">
<#assign patient_symptoms_cds_data = nyha_classification_entry_cds_data + "[0].elements['/data{at0001}/items{at0002}']">

<script> 
angular.module('cds_app').controller('NYHA_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
    $scope.${what_is_the_objective_assessment___ehr_data}.value.selection = $scope.findMin($scope.${what_is_the_objective_assessment___def});
	$scope.${what_are_the_patient_symptoms___ehr_data}.value.selection = $scope.findMin($scope.${what_are_the_patient_symptoms___def});
   });
});
</script>

<div ng-show="ready" ng-controller="NYHA_Ctrl">
<div class="header">
  <h1>{{def.concept.name}}</h1>
  <h2>{{def.concept.description}}</h2>
  <div class="help-tip">
	<p>{{${app_help_tag}.description}}</p>
  </div>
</div>

<div class="card">
  <div class="row collapse" title="{{${what_is_the_objective_assessment___def}.description}}">
    <div class="medium-5 columns">
      <label>{{${what_is_the_objective_assessment___def}.name}}</label>
      </div>
      <label class="medium-7 columns">
        <select ng-model="${what_is_the_objective_assessment___ehr_data}.value.selection" ng-options="key as (key.charAt(0) + ' - ' + desc) for (key, desc) in ${what_is_the_objective_assessment___def}.selections" ng-change="execute()" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }">
        </select>
      </label>
    </div>
	<div class="row collapse" title="{{${what_are_the_patient_symptoms___def}.description}}">
    <div class="medium-5 columns">
      <label>{{${what_are_the_patient_symptoms___def}.name}}</label>
      </div>
      <label class="medium-7 columns">
        <select ng-model="${what_are_the_patient_symptoms___ehr_data}.value.selection" ng-options="key as (key.charAt(0) + ' - ' + desc) for (key, desc) in ${what_are_the_patient_symptoms___def}.selections" ng-change="execute()" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }">
        </select>
      </label>
    </div>
<div class="row collapse" title="{{${objective_assessment_def}.description}}" id="displayresult1">
  <div class="medium-5 columns">
    <label><b>{{${objective_assessment_def}.name}}: </b></label>
  </div>
  <div class="medium-7 columns" style="text-align: center;">
    <label>{{${objective_assessment_cds_data}.value.symbol.value}}</label>
  </div>
</div>
<div class="row collapse" title="{{${patient_symptoms_def}.description}}" id="displayresult2">
  <div class="medium-5 columns">
    <label><b>{{${patient_symptoms_def}.name}}: </b></label>
  </div>
  <div class="medium-7 columns" style="text-align: center;">
    <label>{{${patient_symptoms_cds_data}.value.symbol.value}}</label>
  </div>
</div>
</div>

<div class="row"><div class="medium-12 columns"></div></div>
<br /><br />
<div class="row collapse" title="{{${references}.text}}" style="font-size: 85%;">
  <div class="medium-11 medium-offset-1 column">
    <label>{{${references}.text}}:</label>
  </div>
</div>
