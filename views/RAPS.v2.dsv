<script> 
angular.module('cds_app').controller('RAPS_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
	$scope.${fluid_intake_ehr_data}.value.selection = $scope.findMin($scope.${fluid_intake_def});
	$scope.${food_intake_ehr_data}.value.selection = $scope.findMin($scope.${food_intake_def});
	$scope.${friction_and_shear_ehr_data}.value.selection = $scope.findMin($scope.${friction_and_shear_def});
	$scope.${mobility_ehr_data}.value.selection = $scope.findMin($scope.${mobility_def});
	$scope.${general_condition_ehr_data}.value.selection = $scope.findMin($scope.${general_condition_def});
	$scope.${sensory_ability_ehr_data}.value.selection = $scope.findMin($scope.${sensory_ability_def});
	$scope.${moisture_exposure_ehr_data}.value.selection = $scope.findMin($scope.${moisture_exposure_def});
	$scope.${physical_activity_ehr_data}.value.selection = $scope.findMin($scope.${physical_activity_def});
	$scope.${body_temperature_ehr_data}.value.selection = $scope.findMin($scope.${body_temperature_def});
	$scope.${serum_albumin_ehr_data}.value.units = $scope.${serum_albumin_def}.units[1];
	$scope.execute();
  });
  $scope.findMin = function (element) {
		var array = []
		for(selection in element.selections) {
			array.push(selection);
		}
		return (function() {
			var min = 0;
			var result;
			for (i=0; i<array.length; i++) {
				if (array[i].charAt(0) <= min) {
					result = array[i];
				}
			}
			return result;
		})(array);
	};
});
</script>
