	$scope.${pain_occurred_or_worsened_with_inspiration_ehr_data}.value.selection = $scope.findMin($scope.${pain_occurred_or_worsened_with_inspiration_def});
	$scope.${pain_is_reproduced_by_palpation_ehr_data}.value.selection = $scope.findMin($scope.${pain_is_reproduced_by_palpation_def});
	$scope.${diaphoresis_ehr_data}.value.selection = $scope.findMin($scope.${diaphoresis_def});
	$scope.${was_there_new_ischemia_on_ecg__ehr_data}.value.selection = $scope.findMin($scope.${was_there_new_ischemia_on_ecg__def});
	$scope.${was_both_0hr_and_2hr_troponin_negative__ehr_data}.value.selection = $scope.findMin($scope.${was_both_0hr_and_2hr_troponin_negative__def});
  });
});

function getInformation() {
    var x = document.getElementById('infobox');
    if (x.style.display === 'none') {
        x.style.display = 'block';
    } else {
        x.style.display = 'none';
    }

}

</script>

