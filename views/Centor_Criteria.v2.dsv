  <h2>{{def.concept.description}}</h2>
  <div class="help-tip">
  <p>{{${app_help_tag}.description}}</p>
  </div>
</div>
<div class="card">
<div class="row collapse" title="{{${birthdate_def}.description}}">
	<div class="medium-5 columns">
		<label>{{${birthdate_def}.name}}</label>
	</div>
	<div class="medium-7 columns">
		<input ng-model="${birthdate_ehr_data}.value.value" type="text" pikaday="date" ng-change="execute()" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }" />
	</div>
</div>
<div class="row collapse" title="{{${cough__def}.description}}">
    <div class="medium-6 columns">
      <label>{{${cough__def}.name}}</label>
    </div>
    <label class="medium-3 columns">
		<input type="radio" value="{{alternative_absent_value}}" ng-model="${cough__ehr_data}.value.selection" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }" ng-change="execute()" /> {{alternative_absent_desc}}
	</label>
	<label class="medium-3 columns">
		<input type="radio" value="{{alternative_present_value}}" ng-model="${cough__ehr_data}.value.selection" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }" ng-change="execute()" /> {{alternative_present_desc}}
	</label>
  </div>
<div class="row collapse" title="{{${swollen_tender_anterior_cervical_nodes__def}.description}}">
    <div class="medium-6 columns">
      <label>{{${swollen_tender_anterior_cervical_nodes__def}.name}}</label>
    </div>
    <label class="medium-3 columns" ng-repeat="(key, desc) in ${swollen_tender_anterior_cervical_nodes__def}.selections">
      <input type="radio" value="{{key}}" ng-model="${swollen_tender_anterior_cervical_nodes__ehr_data}.value.selection" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }" ng-change="execute()" /> {{desc}}
    </label>
  </div>
<div class="row collapse" title="{{${tonsillar_exudate_or_swelling__def}.description}}">
    <div class="medium-6 columns">
      <label>{{${tonsillar_exudate_or_swelling__def}.name}}</label>
    </div>
    <label class="medium-3 columns" ng-repeat="(key, desc) in ${tonsillar_exudate_or_swelling__def}.selections">
      <input type="radio" value="{{key}}" ng-model="${tonsillar_exudate_or_swelling__ehr_data}.value.selection" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }" ng-change="execute()" /> {{desc}}
    </label>
  </div>
<div class="row collapse" title="{{${body_temperature_entry_def}.description}}">
	<div class="medium-5 columns">
		<label>{{${temperature_def}.name}}</label>
	</div>
	<div class="medium-5 columns">
		<input ng-model="${temperature_ehr_data}.value.magnitude" type="number" step="any" ng-change="execute()" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }" />
	</div>
	<div class="medium-2 columns">
		<select ng-model="${temperature_ehr_data}.value.units" id="temperature_ehr_units" ng-options="unit for unit in ${temperature_def}.units" ng-change="execute()" disabled=""></select>
	</div>
</div>
<div class="row collapse" title="{{${total_score_def}.description}}" id="displayresult1">
  <div class="medium-4 columns">
    <label><b>{{${total_score_def}.name}}: </b></label>
  </div>
  <div class="medium-8 columns end" style="text-align: center;">
    <label>{{${total_score_cds_data}.value.magnitude}}</label>
  </div>
</div>
<div class="row collapse" title="{{${recommendation_def}.description}}" id="displayresult2">
