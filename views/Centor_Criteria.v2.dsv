<script>
  angular.module('cds_app').controller('Centor_Criteria_Ctrl', function($scope, $window){
    $scope.$on('json_loaded', function(){
      $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	  $scope.${event_time2_ehr_data}.value.value = new Date().toISOString();
	  $scope.${event_time3_ehr_data}.value.value = new Date().toISOString();      
      $scope.${swollen_tender_anterior_cervical_nodes__ehr_data}.value.selection = $scope.findMin($scope.${swollen_tender_anterior_cervical_nodes__def});
      $scope.${tonsillar_exudate_or_swelling__ehr_data}.value.selection = $scope.findMin($scope.${tonsillar_exudate_or_swelling__def});
	  $scope.${cough__ehr_data}.value.selection = $scope.findMin($scope.${cough__def});
      $scope.${temperature_ehr_data}.value.units = $scope.${temperature_def}.units[0];
	  $scope.execute();
	  var selections = $scope.${cough__def}.selections;
				Object.keys(selections).forEach((selection) => {
					if(selection.includes("at0010")) {
						$scope.alternative_present_value = selection;
						$scope.alternative_present_desc = selections[selection];
					}
					if(selection.includes("at0011")) {
						$scope.alternative_absent_value = selection;
						$scope.alternative_absent_desc = selections[selection];
					}
				});
    });
    $scope.findMin = function (element) {
        var array = []
        for  (selection in element.selections) {
          array.push(selection);
        }
        return (function() {
          var min = 0;
          var result;
          for (i=0; i<array.length; i++) {
            if (array[i].charAt(0) <= min && array[i].charAt(0) >= 0) { //catch exception of -2
              result = array[i];
            }
          }
          return result;
        })(array);
      };
  });
</script>
<div ng-show="ready" ng-controller="Centor_Criteria_Ctrl">
<div class="header">
