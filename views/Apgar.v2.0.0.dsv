	<script> 
		angular.module('cds_app').controller('Apgar_Ctrl', function($scope, $window){
			$scope.$on('json_loaded', function(){
				$scope.${event_time_ehr_data}.value.value = new Date().toISOString();
				$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
				$scope.${heart_rate1_ehr_data}.value.units = $scope.${ heart_rate1_def}.units[0];
				$scope.${reflex_irritability_ehr_data}.value.selection = $scope.findMax($scope.${reflex_irritability_def});
				$scope.${skin_colour_ehr_data}.value.selection = $scope.findMax($scope.${skin_colour_def});
				$scope.${respiratory_effort_ehr_data}.value.selection = $scope.findMax($scope.${respiratory_effort_def});
				$scope.${muscle_tone_ehr_data}.value.selection = $scope.findMax($scope.${muscle_tone_def});
				$scope.execute();
			});
			$scope.findMax = function (element) {
				var array = [];
				for(selection in element.selections) {
					array.push(selection);
				}
				return (function() {
					var max = Number.MIN_VALUE;
					var result;
					for (var i=0; i<array.length; i++) {
						var value = parseFloat(array[i].split("|")[0]);
						if (value >= max) {
							max = value;
							result = array[i];
						}
					}
					return result;
				})(array);
			};
		});
	</script>

