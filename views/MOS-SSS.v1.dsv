angular.module('cds_app').controller('MOS-SSS_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
    $scope.${someone_you_can_count_on_to_listen_to_you_when_you_need_to_talk_ehr_data}.value.selection = $scope.findMin($scope.${someone_you_can_count_on_to_listen_to_you_when_you_need_to_talk_def});
	$scope.${someone_to_give_you_good_advice_about_a_crisis_ehr_data}.value.selection = $scope.findMin($scope.${someone_to_give_you_good_advice_about_a_crisis_def});
	$scope.${someone_to_confide_in_or_talk_to_about_yourself_or_your_problems_ehr_data}.value.selection = $scope.findMin($scope.${someone_to_confide_in_or_talk_to_about_yourself_or_your_problems_def});
	$scope.${someone_whose_advice_you_really_want_ehr_data}.value.selection = $scope.findMin($scope.${someone_whose_advice_you_really_want_def});
	$scope.${someone_to_share_your_most_private_worries_and_fears_with_ehr_data}.value.selection = $scope.findMin($scope.${someone_to_share_your_most_private_worries_and_fears_with_def});
	$scope.${someone_to_turn_to_for_suggestions_about_how_to_deal_with_a_personal_problem_ehr_data}.value.selection = $scope.findMin($scope.${someone_to_turn_to_for_suggestions_about_how_to_deal_with_a_personal_problem_def});
