<#assign respiration_rate__22__min__def = qsofa_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0005}']">
<#assign respiration_rate__22__min__cds_data = qsofa_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0005}']">
<#assign systolic_blood_pressure__100_mm_hg_def = qsofa_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0006}']">
<#assign systolic_blood_pressure__100_mm_hg_cds_data = qsofa_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0006}']">
<#assign event_time_def = qsofa_entry_def + ".elements['/data/events/time']">
<#assign event_time_cds_data = qsofa_entry_cds_data + "[0].elements['/data/events/time']">
<#assign total_score_def = qsofa_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0007}']">
<#assign total_score_cds_data = qsofa_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0007}']">
<#assign qsofa_assessment_entry_cds_data = "data.cds['openEHR-EHR-EVALUATION.qsofa_assessment.v1']">
<#assign qsofa_assessment_entry_def = "def.entries['openEHR-EHR-EVALUATION.qsofa_assessment.v1']">
<#assign risk_assessment_def = qsofa_assessment_entry_def + ".elements['/data{at0001}/items{at0003}']">
<#assign risk_assessment_cds_data = qsofa_assessment_entry_cds_data + "[0].elements['/data{at0001}/items{at0003}']">
<#assign glasgow_coma_scale_entry_ehr_data = "data.ehr['openEHR-EHR-OBSERVATION.glasgow_coma_scale.v1']">
<#assign glasgow_coma_scale_entry_def = "def.entries['openEHR-EHR-OBSERVATION.glasgow_coma_scale.v1']">
<#assign gcs_total_score_def = glasgow_coma_scale_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0026}']">
<#assign event_time1_def = glasgow_coma_scale_entry_def + ".elements['/data/events/time']">
<#assign gcs_total_score_ehr_data = glasgow_coma_scale_entry_ehr_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0026}']">
<#assign event_time1_ehr_data = glasgow_coma_scale_entry_ehr_data + "[0].elements['/data/events/time']">
<#assign blood_pressure_entry_ehr_data = "data.ehr['openEHR-EHR-OBSERVATION.blood_pressure.v1']">
<#assign blood_pressure_entry_def = "def.entries['openEHR-EHR-OBSERVATION.blood_pressure.v1']">
<#assign systolic_bp_def = blood_pressure_entry_def + ".elements['/data{at0001}/events{at0006}/data{at0003}/items{at0004}']">
<#assign event_time2_def = blood_pressure_entry_def + ".elements['/data/events/time']">
<#assign systolic_bp_ehr_data = blood_pressure_entry_ehr_data + "[0].elements['/data{at0001}/events{at0006}/data{at0003}/items{at0004}']">
<#assign event_time2_ehr_data = blood_pressure_entry_ehr_data + "[0].elements['/data/events/time']">
<#assign respirations_entry_ehr_data = "data.ehr['openEHR-EHR-OBSERVATION.respiration.v1']">
<#assign respirations_entry_def = "def.entries['openEHR-EHR-OBSERVATION.respiration.v1']">
<#assign respiration_rate_def = respirations_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0004}']">
<#assign event_time3_def = respirations_entry_def + ".elements['/data/events/time']">
<#assign respiration_rate_ehr_data = respirations_entry_ehr_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0004}']">
<#assign event_time3_ehr_data = respirations_entry_ehr_data + "[0].elements['/data/events/time']">

<script>
  angular.module('cds_app').controller('qSOFA_Ctrl', function($scope, $window){
    $scope.$on('json_loaded', function(){
		$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
		$scope.${event_time2_ehr_data}.value.value = new Date().toISOString();
		$scope.${event_time3_ehr_data}.value.value = new Date().toISOString();
		$scope.${respiration_rate_ehr_data}.value.units = $scope.${respiration_rate_def}.units[0];
		$scope.${systolic_bp_ehr_data}.value.units = $scope.${systolic_bp_def}.units[0];
		$scope.${gcs_total_score_ehr_data}.value.selection = $scope.findMax($scope.${gcs_total_score_def});
		$scope.qsofaNotHighRisk=true;
		$scope.execute();
    });
      $scope.$on('executeEnd', function() {
        $scope.qsofaNotHighRisk=false;
        $scope.qsofaHighRisk=false;

      if ($scope.${total_score_cds_data}.value.magnitude < 2) {
        $scope.qsofaNotHighRisk=true;
      }
      if ($scope.${total_score_cds_data}.value.magnitude >= 2) {
        $scope.qsofaHighRisk=true;
      }
    });
      $scope.findMax = function (element) {
			var array = [];
			for(selection in element.selections) {
				array.push(selection);
			}
			return (function() {
				var max = Number.MIN_VALUE;
				var result;
				for (var i=0; i<array.length; i++) {
					var value = parseFloat(array[i].split("|")[0]);
					if (value >= max) {
						max = value;
						result = array[i];
					}
				}
				return result;
			})(array);
		};
});
</script>

<div ng-show="ready" ng-controller="qSOFA_Ctrl">
<div class="header">
