<script> 
angular.module('cds_app').controller('CCR_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
    $scope.${q1__age___65_years__extremity_paresthesias_or_dangerous_mechanism__if_no_go_to_q2__ehr_data}.value.selection = $scope.findMax($scope.${q1__age___65_years__extremity_paresthesias_or_dangerous_mechanism__if_no_go_to_q2__def});
	$scope.${q2__low_risk_factor_present__if_yes___low_risk_go_to_q3__ehr_data}.value.selection = $scope.findMax($scope.${q2__low_risk_factor_present__if_yes___low_risk_go_to_q3__def});
	$scope.${q3__able_to_actively_rotate_neck_45__left_and_right_if_no___high_risk_factors_ehr_data}.value.selection = $scope.findMax($scope.${q3__able_to_actively_rotate_neck_45__left_and_right_if_no___high_risk_factors_def});
	$scope.execute();
  });
  $scope.findMax = function (element) {
		var array = [];
		for(selection in element.selections) {
			array.push(selection);
		}
		return (function() {
			var max = Number.MIN_VALUE;
			var result;
			for (var i=0; i<array.length; i++) {
				var value = parseFloat(array[i].split("|")[0]);
				if (value >= max) {
					max = value;
					result = array[i];
				}
			}
			return result;
		})(array);
	};
});
</script>

