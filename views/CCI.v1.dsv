angular.module('cds_app').controller('CCI_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
    $scope.${age_ehr_data}.value.units = $scope.${age_def}.units[0];
	$scope.${myocardial_infarction__history__not_ecg_changes_only__ehr_data}.value.selection = $scope.findMin($scope.${myocardial_infarction__history__not_ecg_changes_only__def});
	$scope.${congestive_heart_failure_ehr_data}.value.selection = $scope.findMin($scope.${congestive_heart_failure_def});
	$scope.${peripheral_disease__includes_aortic_aneurysm____6_cm_ehr_data}.value.selection = $scope.findMin($scope.${peripheral_disease__includes_aortic_aneurysm____6_cm_def});
	$scope.${cerebrovascular_disease__cva_with_mild_or_no_residua_or_tia_ehr_data}.value.selection = $scope.findMin($scope.${cerebrovascular_disease__cva_with_mild_or_no_residua_or_tia_def});
	$scope.${dementia_ehr_data}.value.selection = $scope.findMin($scope.${dementia_def});
