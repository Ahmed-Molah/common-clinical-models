	<script> 
		angular.module('cds_app').controller('Alvarado_Ctrl', function($scope, $window){
			$scope.$on('json_loaded', function(){
				$scope.${event_time_ehr_data}.value.value = new Date().toISOString();
				$scope.${event_time1_ehr_data}.value.value = new Date().toISOString();
				$scope.${event_time2_ehr_data}.value.value = new Date().toISOString();
				$scope.${right_lower_quadrant_tenderness_ehr_data}.value.selection = $scope.findMin($scope.${right_lower_quadrant_tenderness_def});
				$scope.${rebound_tenderness_ehr_data}.value.selection = $scope.findMin($scope.${rebound_tenderness_def});
				$scope.${migration_of_pain_to_the_right_lower_quadrant_ehr_data}.value.selection = $scope.findMin($scope.${migration_of_pain_to_the_right_lower_quadrant_def});
				$scope.${anorexia_ehr_data}.value.selection = $scope.findMin($scope.${anorexia_def});
				$scope.${nausea_or_vomiting_ehr_data}.value.selection = $scope.findMin($scope.${nausea_or_vomiting_def});
				$scope.${leukocyte_left_shift_ehr_data}.value.selection = $scope.findMin($scope.${leukocyte_left_shift_def});
				$scope.${white_cell_count_ehr_data}.value.units = $scope.${white_cell_count_def}.units[0];
				$scope.${temperature_ehr_data}.value.units = $scope.${temperature_def}.units[0];
				$scope.execute();
			});
			$scope.findMin = function (element) {
				var array = []
				for(selection in element.selections) {
					array.push(selection);
				}
				return (function() {
					var min = 0;
					var result;
					for (i=0; i<array.length; i++) {
						if (array[i].charAt(0) <= min) {
							result = array[i];
						}
					}
					return result;
				})(array);
			};
		});
	</script>

