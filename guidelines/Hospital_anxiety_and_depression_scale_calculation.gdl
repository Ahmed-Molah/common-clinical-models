				keywords = <"HADS", "ångest", "depression">
				misuse = <"Används inte för att bedöma annat än patientens ångest och depression.">
				purpose = <"Ett enkelt självskattningsformulär som ger ett mått på patientens sinnesstämning.">
				use = <"För att utvärdera patientens  sinnesstämning och bedöma ångest och depression. Självskattningsformulär.Fär att räkna ut totalsumma för både depression och ångest.">
