				keywords = <"HADS", "anxiety", "depression">
				misuse = <"Do not use to determine other than patients levels of anxiety and depression.Do not use to calculate other than the HADS total scores.">
				purpose = <"An ordinal scale to determine the levels of anxiety and depression that a patient is experiencing.">
				use = <"To determine patients levels of anxiety and depression, when a patient self-reported tool is adequate.To calculate the total score for anxiety and depression.">
			>
