		rules = <
			["gt0021"] = (RULE) <
				when = <"!fired($gt0021)", "$gt0004!=null", "$gt0005!=null", "$gt0006!=null", "$gt0007!=null", "$gt0008!=null", "$gt0009!=null", "$gt0010!=null">
				then = <"$gt0018.magnitude=(((((($gt0004.value+$gt0005.value)+$gt0006.value)+$gt0007.value)+$gt0008.value)+$gt0009.value)+$gt0010.value)",...>
				priority = <8>
			>
			["gt0022"] = (RULE) <
				when = <"!fired($gt0022)", "$gt0011!=null", "$gt0012!=null", "$gt0013!=null", "$gt0014!=null", "$gt0015!=null", "$gt0016!=null", "$gt0017!=null">
				then = <"$gt0019.magnitude=(((((($gt0011.value+$gt0012.value)+$gt0013.value)+$gt0014.value)+$gt0015.value)+$gt0016.value)+$gt0017.value)",...>
				priority = <7>
			>
			["gt0031"] = (RULE) <
				when = <"!fired($gt0031)", "$gt0018>=11">
				then = <"$gt0030=local::at0005|Anxiety|",...>
				priority = <6>
			>
			["gt0032"] = (RULE) <
				when = <"!fired($gt0032)", "$gt0019>=11">
				then = <"$gt0029=local::at0086|Depression|",...>
				priority = <5>
			>
			["gt0033"] = (RULE) <
				when = <"!fired($gt0033)", "$gt0018>=8", "$gt0018<=10">
				then = <"$gt0030=local::at0082|Possible anxiety|",...>
				priority = <4>
			>
			["gt0034"] = (RULE) <
				when = <"!fired($gt0034)", "$gt0019>=8", "$gt0019<=10">
				then = <"$gt0029=local::at0085|Possible depression|",...>
				priority = <3>
			>
			["gt0035"] = (RULE) <
				when = <"!fired($gt0035)", "$gt0018>=0", "$gt0018<=7">
				then = <"$gt0030=local::at0081|No anxiety|",...>
				priority = <2>
			>
			["gt0036"] = (RULE) <
				when = <"!fired($gt0036)", "$gt0019>=0", "$gt0019<=7">
				then = <"$gt0029=local::at0084|No depression|",...>
				priority = <1>
			>
		>
