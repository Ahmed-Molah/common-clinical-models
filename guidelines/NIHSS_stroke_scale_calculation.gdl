		rules = <
			["gt0021"] = (RULE) <
				when = <"!fired($gt0021)", "$gt0020!=null", "$gt0019!=null", "$gt0018!=null", "$gt0017!=null", "$gt0016!=null", "$gt0015!=null", "$gt0014!=null", "$gt0013!=null", "$gt0012!=null", "$gt0011!=null", "$gt0010!=null", "$gt0006!=null", "$gt0007!=null", "$gt0008!=null", "$gt0009!=null">
				then = <"$gt0004.magnitude=(((((((((((((($gt0006.value+$gt0007.value)+$gt0008.value)+$gt0009.value)+$gt0010.value)+$gt0011.value)+$gt0012.value)+$gt0013.value)+$gt0014.value)+$gt0015.value)+$gt0016.value)+$gt0017.value)+$gt0018.value)+$gt0019.value)+$gt0020.value)",...>
				priority = <6>
			>
			["gt0022"] = (RULE) <
				when = <"!fired($gt0022)", "$gt0004>=25">
				then = <"$gt0005=local::at0010|Very severe neurological impairment|",...>
				priority = <5>
			>
			["gt0023"] = (RULE) <
				when = <"!fired($gt0023)", "$gt0004>=15", "$gt0004<=24">
				then = <"$gt0005=local::at0009|Severe impairment|",...>
				priority = <4>
			>
			["gt0024"] = (RULE) <
				when = <"!fired($gt0024)", "$gt0004>=5", "$gt0004<=14">
				then = <"$gt0005=local::at0008|Moderately severe impairment|",...>
				priority = <3>
			>
			["gt0025"] = (RULE) <
				when = <"!fired($gt0025)", "$gt0004>=1", "$gt0004<=4">
				then = <"$gt0005=local::at0007|Mild impairment|",...>
				priority = <2>
			>
			["gt0026"] = (RULE) <
				when = <"!fired($gt0026)", "$gt0004==0">
				then = <"$gt0005=local::at0006|Normal exam|",...>
				priority = <1>
			>
		>
	>
