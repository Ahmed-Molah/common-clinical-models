				then = <"$gt0010=0|local::at0010|Absent|",...>
				priority = <8>
			>
			["gt0020"] = (RULE) <
				when = <"$gt0003==1|local::at0011|Weak or irregular|",...>
				then = <"$gt0010=1|local::at0011|Weak or irregular|",...>
				priority = <9>
			>
			["gt0021"] = (RULE) <
				when = <"$gt0006==0|local::at0018|No response|",...>
				then = <"$gt0013=0|local::at0018|No response|",...>
				priority = <10>
			>
			["gt0022"] = (RULE) <
				when = <"$gt0006==1|local::at0019|Reduced response|",...>
				then = <"$gt0013=1|local::at0019|Reduced response|",...>
				priority = <11>
			>
			["gt0023"] = (RULE) <
				when = <"$gt0007==0|local::at0022|Completely blue|",...>
				then = <"$gt0014=0|local::at0022|Completely blue|",...>
				priority = <3>
			>
			["gt0024"] = (RULE) <
				when = <"$gt0007==1|local::at0023|Body pink; extremities blue|",...>
				then = <"$gt0014=1|local::at0023|Body pink; extremities blue|",...>
				priority = <2>
			>
