		rules = <
			["gt0008"] = (RULE) <
				when = <"$gt0010==null", "$gt0011==null", "$gt0012==null", "$gt0013==null", "$gt0014==null">
				then = <"$gt0011=2|local::at0008|â‰¥100 beats per minute|", "$gt0012=2|local::at0016|Normal tone|", "$gt0013=2|local::at0020|Normal response|", "$gt0014=2|local::at0024|Completely pink|", "$gt0010=2|local::at0012|Normal|">
				priority = <12>
			>
			["gt0015"] = (RULE) <
				when = <"$gt0004==1|local::at0007|<100 beats per minute|",...>
				then = <"$gt0011=1|local::at0007|<100 beats per minute|",...>
				priority = <6>
			>
			["gt0016"] = (RULE) <
				when = <"$gt0004==0|local::at0006|Absent|",...>
				then = <"$gt0011=0|local::at0006|Absent|",...>
				priority = <7>
